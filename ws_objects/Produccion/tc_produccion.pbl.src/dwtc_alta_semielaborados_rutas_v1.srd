 retrieve="SELECT ruta_fabricacion.articulo,
 		 ruta_fabricacion.empresa,
       ruta_fabricacion.articulo_anterior,
	    A2.descripcion,
       ruta_fabricacion.tipooperacion,
		 tipooperacion.descripcion,
		 ruta_fabricacion.ruta,
		 ruta_descripcion.descripcion,
       ruta_fabricacion.factor,
		 ruta_fabricacion.prioridad,
		 ruta_fabricacion.observaciones,
		 ruta_fabricacion.juegopantallas,
		 juegospantallas.descripcion 
  FROM ruta_fabricacion
		 INNER JOIN ruta_descripcion ON ruta_fabricacion.empresa = ruta_descripcion.empresa AND ruta_fabricacion.ruta = ruta_descripcion.codigo 
		 INNER JOIN tipooperacion ON ruta_fabricacion.empresa = tipooperacion.empresa AND ruta_fabricacion.tipooperacion = tipooperacion.codigo 
		 INNER JOIN articulos A2 ON ruta_fabricacion.empresa = A2.empresa AND ruta_fabricacion.articulo_anterior = A2.codigo 
		 LEFT JOIN juegospantallas ON ruta_fabricacion.empresa = juegospantallas.empresa AND ruta_fabricacion.juegopantallas = juegospantallas.codigo 
WHERE  	ruta_fabricacion.empresa = :empresa AND 
			ruta_fabricacion.articulo = :codigo 
ORDER BY ruta_fabricacion.prioridad ASC, ruta_fabricacion.tipooperacion DESC, ruta_fabricacion.articulo_anterior ASC" update="ruta_fabricacion" updatewhere=0 updatekeyinplace=no arguments=(("empresa", string),("codigo", string)) )
