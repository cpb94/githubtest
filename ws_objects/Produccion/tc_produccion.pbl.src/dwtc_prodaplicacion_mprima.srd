 retrieve="SELECT  	prodaplicacion_mprima.empresa,
			prodaplicacion_mprima.aplicacion,
			prodaplicacion_mprima.mprima,
			prodaplicacion_mprima.proveedor,
			prodaplicacion_mprima.unidad,
			prodaplicacion_mprima.orden,
			prodaplicaciones.descripcion,
			prodaplicaciones.resumido,
			venmprima.descripcion,
			genter.razon,
			comunimedida.descripcion 
FROM 	prodaplicacion_mprima 
			LEFT JOIN comunimedida ON comunimedida.codigo = prodaplicacion_mprima.unidad 
			INNER JOIN prodaplicaciones ON prodaplicacion_mprima.empresa = prodaplicaciones.empresa AND prodaplicacion_mprima.aplicacion = prodaplicaciones.codigo 
			INNER JOIN venmprima ON prodaplicacion_mprima.empresa = venmprima.empresa AND prodaplicacion_mprima.mprima = venmprima.codigo 
			INNER JOIN genter ON prodaplicacion_mprima.empresa = genter.empresa AND prodaplicacion_mprima.proveedor = genter.codigo AND genter.tipoter = 'P'  
WHERE  	prodaplicacion_mprima.empresa = :empresa AND 
			prodaplicacion_mprima.aplicacion = :codigo" update="prodaplicacion_mprima" updatewhere=0 updatekeyinplace=no arguments=(("empresa", string),("codigo", string))  sort="prodaplicacion_mprima_orden A " )
