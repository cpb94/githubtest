 retrieve="SELECT  	tipooperacion_ruta_descripcion.empresa,
			tipooperacion_ruta_descripcion.tipooperacion,
			tipooperacion_ruta_descripcion.ruta_descripcion,
			ruta_descripcion.descripcion
FROM 	   tipooperacion_ruta_descripcion 
			INNER JOIN tipooperacion ON tipooperacion_ruta_descripcion.empresa = tipooperacion.empresa AND tipooperacion_ruta_descripcion.tipooperacion = tipooperacion.codigo 
			INNER JOIN ruta_descripcion ON tipooperacion_ruta_descripcion.empresa = ruta_descripcion.empresa AND tipooperacion_ruta_descripcion.ruta_descripcion = ruta_descripcion.codigo 
