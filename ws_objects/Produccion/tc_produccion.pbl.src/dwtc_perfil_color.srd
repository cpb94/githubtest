 retrieve="  SELECT perfil_color.codigo,   
         perfil_color.empresa,    
         perfil_color.descripcion,   
         perfil_color.interfaz,   
         perfil_color.esmalte,   
         perfil_color.pasta,   
         perfil_color.fecha,   
         perfil_color.fecha_actualizacion,   
         perfil_color.base,   
         perfil_color.nombre_fisico, 
		perfil_color.nuevo_perfil, 
		perfil_color.ruta_conversion,
		perfil_color.resolucion,				
		A.descripcion AS base_descripcion, 
		F1.descripcion_laboratorio AS interfaz_descripcion, 
		F2.descripcion_laboratorio AS esmalte_descripcion,
		P2.descripcion
		
    FROM perfil_color   
         LEFT JOIN articulos A ON perfil_color.empresa = A.empresa AND perfil_color.base = A.codigo 
         LEFT JOIN prodformula F1 ON perfil_color.empresa = F1.empresa AND perfil_color.interfaz = F1.formula 
		LEFT JOIN prodformula F2 ON perfil_color.empresa = F2.empresa AND perfil_color.esmalte = F2.formula 
		LEFT JOIN perfil_color P2 ON perfil_color.empresa = P2.empresa AND perfil_color.nuevo_perfil = P2.codigo 
   WHERE ( perfil_color.empresa = :empresa ) AND  
         ( perfil_color.codigo = :codigo )    
" update="perfil_color" updatewhere=0 updatekeyinplace=no arguments=(("empresa", string),("codigo", string)) )
