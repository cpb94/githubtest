table(column=(type=char(5) updatewhereclause=yes name=plato dbname="prodplatos.plato" )
 column=(type=char(50) updatewhereclause=yes name=prodplatos_descripcion dbname="prodplatos.descripcion" )
 column=(type=decimal(21) updatewhereclause=yes identity=yes name=importe dbname="importe" )
 retrieve="SELECT prodplatos.codigo as plato, 
prodplatos.descripcion, 
(SELECT SUM (venlifac.importe/venlifac.cambio) FROM articulos INNER JOIN venlifac ON articulos.empresa = venlifac.empresa AND articulos.codigo = venlifac.articulo WHERE articulos.uso = '3'  AND prodplatos.codigo = articulos.plato_inf AND articulos.molde = :molde AND CONVERT(date, venlifac.ffactura) >= :desde AND CONVERT(date, venlifac.ffactura) <= :hasta ) as importe 
FROM prodplatos
WHERE prodplatos.tipo = 'Inferior' 
AND (SELECT SUM (venlifac.importe/venlifac.cambio) FROM articulos INNER JOIN venlifac ON articulos.empresa = venlifac.empresa AND articulos.codigo = venlifac.articulo WHERE articulos.uso = '3'  AND prodplatos.codigo = articulos.plato_inf AND CONVERT(date,venlifac.ffactura) >= :desde AND CONVERT(date,venlifac.ffactura) <= :hasta ) > 0 
AND prodplatos.codigo IN (SELECT A.plato_inf FROM articulos A WHERE A.uso = '3' AND A.molde = :molde)" arguments=(("molde", string),("desde", string),("hasta", string))  sort="importe D " )
