 retrieve="SELECT  	prodlineas_perfil_color.empresa,
			prodlineas_perfil_color.prodlineas,
			prodlineas_perfil_color.perfil_color,
			prodlineas_perfil_color.contador,
			prodlineas_perfil_color.carpeta,
			prodlineas_perfil_color.activo,
			perfil_color.descripcion


FROM 	prodlineas_perfil_color 
			INNER JOIN perfil_color ON prodlineas_perfil_color.empresa = perfil_color.empresa AND prodlineas_perfil_color.perfil_color = perfil_color.codigo 
WHERE  	prodlineas_perfil_color.empresa = :empresa AND 
			prodlineas_perfil_color.prodlineas = :codigo" update="prodlineas_perfil_color" updatewhere=0 updatekeyinplace=no arguments=(("empresa", string),("codigo", string)) )
