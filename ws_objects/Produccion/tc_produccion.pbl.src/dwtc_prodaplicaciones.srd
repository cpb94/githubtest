 retrieve="  SELECT prodaplicaciones.codigo,   
         prodaplicaciones.empresa,   
         prodaplicaciones.descripcion,   
         prodaplicaciones.resumido,   
         prodaplicaciones.formula_estandar,   
         prodaplicaciones.formula_unidad,   
         comunimedida.descripcion  ,
         prodaplicaciones.coste_euros_m2

    FROM prodaplicaciones LEFT JOIN comunimedida ON  prodaplicaciones.formula_unidad = comunimedida.codigo
   WHERE ( prodaplicaciones.empresa = :empresa ) AND  
         ( prodaplicaciones.codigo = :codigo )     
" update="prodaplicaciones" updatewhere=0 updatekeyinplace=yes arguments=(("empresa", string),("codigo", string)) )
