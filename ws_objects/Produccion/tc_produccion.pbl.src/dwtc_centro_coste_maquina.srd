 retrieve="SELECT  	centro_coste_maquina.empresa,
			centro_coste_maquina.centro_coste,
			centro_coste_maquina.maquina,
			centro_coste.descripcion,
			maquina.abreviado,
			maquina.descripcion
FROM 		centro_coste_maquina 
			INNER JOIN centro_coste ON centro_coste_maquina.empresa = centro_coste.empresa AND centro_coste_maquina.centro_coste = centro_coste.codigo 
			INNER JOIN maquina ON centro_coste_maquina.empresa = maquina.empresa AND centro_coste_maquina.maquina = maquina.codigo 
WHERE  	centro_coste_maquina.empresa = :empresa AND 
			centro_coste_maquina.centro_coste = :codigo" update="centro_coste_maquina" updatewhere=0 updatekeyinplace=no arguments=(("empresa", string),("codigo", string)) )
