global function string f_tipo_linea (string empresa, string linea);string v_tipo_linea

select tipo_linea
into   :v_tipo_linea
from   prodlineas
where  empresa = :empresa
and    codigo  = :linea;

if isnull(v_tipo_linea) then v_tipo_linea = ""

return v_tipo_linea
end function

