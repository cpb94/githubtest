global function string f_nombre_linea (string empresa, string linea);string v_nom

select descripcion
into   :v_nom
from   prodlineas
where  empresa = :empresa
and    codigo  = :linea;

if sqlca.sqlcode <> 0 then
	v_nom = ""
end if

if isnull(v_nom) then v_nom = ""

return v_nom
end function

