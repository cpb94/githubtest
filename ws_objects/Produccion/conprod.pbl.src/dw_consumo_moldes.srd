table(column=(type=char(10) updatewhereclause=yes name=articulos_molde dbname="articulos.molde" )
 column=(type=char(50) updatewhereclause=yes name=prodmoldes_descripcion dbname="prodmoldes.descripcion" )
 column=(type=decimal(0) updatewhereclause=yes name=piezas dbname="Piezas" )
 column=(type=number updatewhereclause=yes name=metros dbname="Metros" )
 retrieve="
  SELECT articulos.molde,   
         prodmoldes.descripcion  ,
	sum(prodpartes_ot.buenas + prodpartes_ot.malas) Piezas,
	sum((prodpartes_ot.buenas + prodpartes_ot.malas) * formatos.largo *formatos.ancho /10000)  Metros

    FROM prodpartes_ot,   
         articulos,   
         formatos,   
         prodmoldes  
   WHERE ( prodpartes_ot.empresa = articulos.empresa ) and  
         ( formatos.empresa = prodmoldes.empresa ) and  
         ( prodpartes_ot.articulo = articulos.codigo ) and  
         ( prodpartes_ot.formato = formatos.codigo ) and  
		( prodpartes_ot.ordenoperacion = 1) and
         ( articulos.molde = prodmoldes.codigo ) and  
			articulos.uso <> '1' and
         ( ( articulos.molde like :molde ) AND  
		prodpartes_ot.fecha >= :desde and
		prodpartes_ot.fecha <= :hasta and
         ( prodpartes_ot.empresa = :empresa ) )  
group by articulos.molde,   
         prodmoldes.descripcion 
order by Piezas desc

/*
  SELECT articulos.molde,   
         prodmoldes.descripcion  ,
	sum(prodpartes_ot.buenas + prodpartes_ot.malas) Piezas,
	sum((prodpartes_ot.buenas + prodpartes_ot.malas) * formatos.largo *formatos.ancho /10000)  Metros

    FROM prodpartes_ot,   
         articulos,   
         art_codestadistico,   
         formatos,   
         prodmoldes  
   WHERE ( prodpartes_ot.empresa = articulos.empresa ) and  
         ( articulos.empresa = art_codestadistico.empresa ) and  
         ( art_codestadistico.empresa = formatos.empresa ) and  
         ( formatos.empresa = prodmoldes.empresa ) and  
         ( prodpartes_ot.articulo = articulos.codigo ) and  
         ( articulos.codigo = art_codestadistico.codigo ) and  
         ( prodpartes_ot.formato = formatos.codigo ) and  
		( prodpartes_ot.ordenoperacion = 1) and
         ( articulos.molde = prodmoldes.codigo ) and  
		art_codestadistico.g2 <> 'Z' and
		art_codestadistico.g2 <> 'B' and
		art_codestadistico.g2 <> 'N' and
         ( ( articulos.molde like :molde ) AND  
		prodpartes_ot.fecha >= :desde and
		prodpartes_ot.fecha <= :hasta and
         ( prodpartes_ot.empresa = :empresa ) )  
group by articulos.molde,   
         prodmoldes.descripcion 
order by Piezas desc
*/" arguments=(("empresa", string),("molde", string),("desde", datetime),("hasta", datetime)) )
