 column=(type=long updatewhereclause=yes name=juegospantallas_ubicacion dbname="juegospantallas.ubicacion" )
 column=(type=char(40) updatewhereclause=yes name=juegospantallas_descripcion dbname="juegospantallas.descripcion" )
 retrieve="  SELECT art_ver_operaciones.articulo,   
         articulos.descripcion,   
         art_ver_operaciones.version,   
         art_ver_operaciones.orden,   
         juegospantallas.codigo,
         juegospantallas.ubicacion,
		juegospantallas.descripcion
     FROM art_ver_operaciones,   
         articulos,   
		juegospantallas

	where juegospantallas.empresa = :ar_empresa 
	and juegospantallas.codigo = :ar_pantalla
	and juegospantallas.codigo = art_Ver_operaciones.juegopantallas
	and juegospantallas.empresa = art_ver_operaciones.empresa
	and art_ver_operaciones.articulo = articulos.codigo
	and art_ver_operaciones.empresa = articulos.empresa

	order by convert(integer,  art_ver_operaciones.articulo)

" arguments=(("ar_empresa", string),("ar_pantalla", string)) )
