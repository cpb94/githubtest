 retrieve="select  	art_escandallo.empresa, 
			art_escandallo.articulo_ant, 
			articulos.descripcion, sum(buenas + malas),	
			formatos.largo * formatos.ancho / 10000 as superficie ,
			formatos.abreviado,
			art_codestadistico.G2,
			(select sum (cantidade -cantidads )
			from almacenmovimientos
			where empresa = art_escandallo.empresa
			and articulo = art_escandallo.articulo_ant) as existencias,
			0 coste

from  prodpartes_ot, art_escandallo, articulos, formatos, art_codEstadistico
where prodpartes_ot.empresa = :empresa
and prodpartes_ot.ordenoperacion = 1
and art_escandallo.empresa = :empresa
and art_codestadistico.empresa = :empresa
and art_escandallo.articulo_ant = art_codestadistico.codigo
and articulos.empresa = :empresa
and art_escandallo.articulo *= prodpartes_ot.articulo 
and art_escandallo.articulo_ant = articulos.codigo
and art_escandallo.articulo_ant <> 0
and articulos.uso = 1
and formatos.empresa = :empresa
and articulos.formato = formatos.codigo
and ( prodpartes_ot.fecha >= :fecha1 ) 
and ( prodpartes_ot.fecha <= :fecha2 ) 
and articulos.codigo like :articulo
and ((articulos.molde like :molde) or (articulos.molde is null) or (articulos.molde = ''))
and articulos.formato like :formato
group by 	art_escandallo.empresa,
				art_escandallo.articulo_ant, 
				articulos.descripcion, 
				formatos.largo * formatos.ancho / 10000 , 
				formatos.abreviado,
				art_codestadistico.G2
having sum(buenas + malas) = 0 or sum(buenas + malas) is null

union

select distinct articulos.empresa,
					convert (integer, articulos.codigo), 
					articulos.descripcion, 
					0, 
					formatos.largo * formatos.ancho / 10000 as superficie,
					formatos.abreviado,
					art_codestadistico.G2,
					(select sum (cantidade - cantidads )
					from almacenmovimientos
					where empresa = articulos.empresa
					and articulo = articulos.codigo) as existencias,
					0 coste
from articulos, formatos, art_codestadistico
where articulos.empresa = :empresa
and articulos.empresa = :empresa
and art_codestadistico.empresa = :empresa
and art_codestadistico.codigo = articulos.codigo
and articulos.codigo like :articulo 
and ((articulos.molde like :molde) or (articulos.molde is null) or (articulos.molde = '')) 
and articulos.formato like :formato    
and art_codestadistico.tm like :tipopieza
and articulos.formato = formatos.codigo
and	articulos.codigo NOT IN  (SELECT art_escandallo.articulo_ant
											FROM art_escandallo 
											where art_escandallo.empresa = :empresa
											and art_escandallo.articulo_ant = articulos.codigo)
and articulos.uso = 1;





