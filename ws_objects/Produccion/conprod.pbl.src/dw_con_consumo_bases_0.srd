 column=(type=char(10) updatewhereclause=yes name=formatos_abreviado dbname="formatos.abreviado" dbalias=".abreviado" )
 column=(type=char(1) updatewhereclause=yes name=art_codestadistico_g2 dbname="art_codestadistico.G2" dbalias=".G2" )
