integer x = 4946
integer y = 256
integer width = 352
integer height = 100
boolean bringtotop = true
integer textsize = -8
integer weight = 700
fontcharset fontcharset = ansi!
fontpitch fontpitch = variable!
fontfamily fontfamily = swiss!
string facename = "Arial"
string text = "Trasp. tarifas"
end type

event clicked;long i, total
string articulo, articulo_ant, formato, descripcion
dec precio
datetime fecha
boolean exito
if MessageBox("Pregunta", "Continuar",  Exclamation!, YesNo!, 2) = 1 then
	for i = 1 to dw_1.rowcount()
		st_tarifa.text = string(i)+"/"+string(dw_1.rowcount())
		exito = true
		articulo = dw_1.object.articulos_codigo[i]
		select precio
		into :precio
		from  venlintarifas
		where empresa = :codigo_empresa
		and articulo = :articulo
		and tarifa = '5';
		
		select articulo_ant
		into :articulo_ant
		from art_escandallo, articulos
		where art_escandallo.empresa = :codigo_Empresa
		and art_escandallo.articulo = articulos.codigo
		and art_escandallo.empresa = articulos.empresa
		and articulos.uso <> '1'
		and articulo = :articulo;
		
		select formato, descripcion
		into :formato, :descripcion
		from articulos
		where empresa = :codigo_empresa
		and codigo = :articulo_ant;
		
		fecha = datetime (today())
		connect using SQLCA;
		
		select count(*)
		into :total
		from	venlintarifas 
		where empresa = '1' 
		and tarifa = '5'
		and articulo = :articulo_ant;
		
		if total <= 0 then
		
			insert into venlintarifas ( empresa, tarifa, fecha, articulo, calidad, precio, falta, montajeartcal, familia, formato, activo, 
												 listar, usuario, descripcion, excepcion, tipo_linea)
			values							( '1', '5', '11-6-2001', :articulo_ant, '1', :precio, :fecha, :articulo_ant+'01', '1', :formato, 'S',
												'S', :nombre_usuario, :descripcion, 'N', '2');
										
			if SQLCA.SQlCode = -1 then
				exito = false
			end if
										
			if exito then
				commit using SQLCA;
			else
				rollback using SQLCA;
				messagebox ("ATENCIÓN, ATENCIÓN ............", "No toques nada, no le des al botón Ok ni a ningún otro. ~r Avisa al administrador. Articulo = "+articulo+" anterior = "+articulo_Ant)
			end if
		else
			if articulo_ant <> '0' then
				messagebox ("Articulo Principal = " +articulo, " El artículo "+articulo_ant+" ya tiene tarifa de valoración de stock ")
			end if
		end if
	next
end if

messagebox ("Aviso", "THE END")

end event

