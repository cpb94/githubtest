This.SelectRow(0, FALSE)	
This.SelectRow ( currentrow, True )

p_imagen.PictureName = ""
dw_imagen.visible = false
IF dw_1.GetRow() > 0 then
	modelo = dw_1.object.articulos_modelo_origen[dw_1.getrow()]
	if not isnull(modelo) and modelo <> "" then
		articulo = dw_1.object.articulos_codigo[dw_1.getrow()]
		ftc_imagen_articulo("5",articulo,dw_imagen)	
	else
		p_imagen.PictureName = f_cargar_imagen_nuevo(string(dw_1.object.articulos_codigo_pantallas[dw_1.GetRow()]))
	end if
END IF

end event

