table(column=(type=char(5) updatewhereclause=yes name=art_escandallo_empresa dbname="art_escandallo.empresa" dbalias=".empresa" )
 column=(type=char(1) updatewhereclause=yes name=art_codestadistico_b_g2 dbname="art_codestadistico.G2" dbalias=".G2" )
 column=(type=char(20) updatewhereclause=yes name=art_escandallo_articulo_ant dbname="art_escandallo.articulo_ant" dbalias=".articulo_ant" )
 column=(type=char(10) updatewhereclause=yes name=formatos_abreviado dbname="formatos.abreviado" dbalias=".abreviado" )
 column=(type=char(20) updatewhereclause=yes name=articulos_codigo dbname="articulos.codigo" dbalias=".codigo" )
 column=(type=char(40) updatewhereclause=yes name=articulos_descripcion dbname="articulos.descripcion" dbalias=".descripcion" )
 column=(type=char(10) updatewhereclause=yes name=articulos_molde dbname="articulos.molde" dbalias=".molde" )
 column=(type=decimal(6) updatewhereclause=yes identity=yes name=piezas dbname="piezas" )
 column=(type=decimal(6) updatewhereclause=yes identity=yes name=metros dbname="Metros" )
 column=(type=decimal(6) updatewhereclause=yes identity=yes name=existencias dbname="existencias" )
 column=(type=decimal(0) updatewhereclause=yes identity=yes name=consumo_programado dbname="consumo_programado" )
 retrieve="select  	art_escandallo.empresa,
			art_codestadistico.G2,
			art_escandallo.articulo_ant, 
			formatos.abreviado,
			articulos.codigo,
			articulos.descripcion,
			articulos.molde,
			sum ( ( prodpartes_ot.buenas + prodpartes_ot.malas) / (art_escandallo.factor / art_ver_operaciones.factor)) as piezas,
			round(sum ( ( prodpartes_ot.buenas + prodpartes_ot.malas) / (art_escandallo.factor / art_ver_operaciones.factor) * ( formatos.largo * formatos.ancho / 10000)), 0) as Metros,
			(select sum (cantidade - cantidads)
			from almacenmovimientos
			where empresa = art_escandallo.empresa
		   and articulo = art_escandallo.articulo_ant) as existencias,
			(select sum (cantidad )
			from planificacion 
			where empresa = art_escandallo.empresa
			and ordenoperacion = '1'
			and art_escandallo.articulo_ant = planificacion.base) as consumo_programado


from 		art_escandallo, prodpartes_ot, articulos, formatos, art_codestadistico, art_ver_operaciones
where 	art_escandallo.empresa = 1 and
			art_escandallo.articulo = prodpartes_ot.articulo and
			prodpartes_ot.empresa = art_escandallo.empresa and
			prodpartes_ot.articulo = art_escandallo.articulo and
			prodpartes_ot.ordenoperacion = 1 and
			art_escandallo.empresa = articulos.empresa and
			art_escandallo.articulo_ant = articulos.codigo and
			articulos.empresa = formatos.empresa and
			articulos.formato = formatos.codigo and
			art_escandallo.empresa = art_codestadistico.empresa and
			art_escandallo.articulo_ant = art_codestadistico.codigo and
			art_escandallo.empresa = art_ver_operaciones.empresa and
			art_escandallo.articulo = art_ver_operaciones.articulo and
			art_escandallo.version = art_ver_operaciones.version and
			( art_codestadistico.G2 = 'B' or art_codestadistico.G2 = 'Z') and
			art_ver_operaciones.orden = 1 and
			articulos.control_stock = 'S' and
         ( prodpartes_ot.empresa = :empresa ) AND  
         ( prodpartes_ot.fecha >= :fecha1 ) AND  
         ( prodpartes_ot.fecha <= :fecha2 ) AND  
			articulos.control_stock = 'S' and
		(( prodpartes_ot.ajuste is null) OR ( prodpartes_ot.ajuste = 'N' )) AND
         articulos.codigo like :articulo AND  
         ((articulos.molde like :molde) or (articulos.molde is null) or (articulos.molde = '')) AND  
         articulos.relieve like :tipopieza AND  
         articulos.formato like :formato    


group by	art_escandallo.empresa,
				art_codestadistico.G2,
				articulo_ant, 
				formatos.abreviado,
				articulos.codigo,
				articulos.descripcion,
				articulos.molde,
				formatos.largo * formatos.ancho / 10000 


