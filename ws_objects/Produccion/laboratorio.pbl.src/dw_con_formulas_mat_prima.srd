 retrieve="  SELECT proddetformula.formulacion,   
         prodformula.descripcion_produccion,   
         proddetformula.mp,   
         proddetformula.empresa,   
         proddetformula.proveedor,   
         proddetformula.cantidad,   
         proddetformula.unidadmedida,   
         proddetformula.cantidad_u_m_mp,   
         proddetformula.unidad_u_m_mp,
			venmprima.descripcion,
			prodformula.fecha_alta  
  FROM proddetformula 
  LEFT OUTER JOIN prodformula ON proddetformula.empresa = prodformula.empresa AND proddetformula.formulacion = prodformula.formula
  LEFT OUTER JOIN venmprima ON proddetformula.empresa = venmprima.empresa AND proddetformula.mp = venmprima.codigo  
  WHERE ( proddetformula.empresa = :ar_empresa  ) AND  
         ( proddetformula.mp like :ar_mat_prima )    
" arguments=(("ar_empresa", string),("ar_mat_prima", string))  sort="prodformula_descripcion_produccion A " )
