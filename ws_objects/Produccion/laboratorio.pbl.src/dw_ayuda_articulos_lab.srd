			 	articulos_lab.empresa ,           
				articulos_lab.formato ,           
				articulos_lab.pasta ,           
				articulos_lab.esmalte ,           
				articulos_lab.fecha_alta ,           
				articulos_lab.piezas_juego ,           
				articulos_lab.tipo_acabado ,           
				articulos_lab.ref_disenyo ,           
				articulos_lab.ref_disenyo2     
        FROM articulos_lab      
