 retrieve="  SELECT art_ver_operaciones.empresa,   
         art_ver_operaciones.articulo,   
         art_ver_operaciones.version,   
         art_ver_operaciones.operacion,   
         art_ver_operaciones.tipo_operacion,   
         art_ver_operaciones.orden,   
         art_ver_tipooperacion.descripcion_abr,   
         art_ver_operaciones.formato,   
         art_ver_operaciones.juegopantallas,   
         juegospantallas.descripcion,   
         art_ver_operaciones.factor,   
         formatos.descripcion,   
         art_ver_operaciones.numaplicaciones,   
         formatos.abreviado,  
			formatos.largo, 
			formatos.ancho,
         art_ver_operaciones.coste,   
         formatos.ancho,   
         formatos.largo,   
         art_ver_operaciones.coste_fab,   
         art_ver_operaciones.coste_mp,
         art_ver_operaciones.coste_packing,   
         art_ver_operaciones.coste_compras
    FROM art_ver_operaciones LEFT OUTER JOIN art_ver_tipooperacion ON art_ver_operaciones.empresa = art_ver_tipooperacion.empresa AND art_ver_operaciones.tipo_operacion = art_ver_tipooperacion.codigo LEFT OUTER JOIN formatos ON art_ver_operaciones.empresa = formatos.empresa AND art_ver_operaciones.formato = formatos.codigo LEFT OUTER JOIN juegospantallas ON art_ver_operaciones.empresa = juegospantallas.empresa AND art_ver_operaciones.juegopantallas = juegospantallas.codigo  
   WHERE ( art_ver_operaciones.empresa = :empresa ) AND  
         ( art_ver_operaciones.articulo = :articulo ) AND  
         ( art_ver_operaciones.version = :version )    
" update="art_ver_operaciones" updatewhere=0 updatekeyinplace=yes arguments=(("empresa", string),("articulo", string),("version", string))  sort="integer(orden) A " )
