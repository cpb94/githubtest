 retrieve="  SELECT art_ver_op_aplicaciones.empresa,   
         art_ver_op_aplicaciones.articulo,   
         art_ver_op_aplicaciones.version,   
         art_ver_op_aplicaciones.operacion,   
         art_ver_op_aplicaciones.contadoraplicacion,   
         art_ver_op_aplicaciones.pantalla,   
         art_ver_op_aplicaciones.formula,   
         art_ver_op_aplicaciones.gramaje,   
         art_ver_op_aplicaciones.tipoaplicacion,   
         art_ver_op_aplicaciones.orden,   
         art_ver_op_aplicaciones.densidad,   
         art_ver_op_aplicaciones.viscosidad,   
         art_ver_op_aplicaciones.boquilla,   
         art_ver_op_aplicaciones.ciclo,   
         art_ver_op_aplicaciones.temp_sup,   
         art_ver_op_aplicaciones.temp_inf,   
         art_ver_op_aplicaciones.cortes,   
         art_ver_op_aplicaciones.discos,   
         art_ver_op_aplicaciones.tiff,   
         prodaplicaciones.resumido,   
         prodformula.descripcion_produccion,   
         prodpantallas.descripcion,   
         prodpantallas.numero_hilos,   
         art_ver_op_aplicaciones.gram_7x20,   
         prodpantallas.paso_cliche,
		0 suplemento , 0 peso_total, planificacion.cantidad	  
    FROM art_ver_op_aplicaciones 
LEFT OUTER JOIN prodaplicaciones ON art_ver_op_aplicaciones.empresa = prodaplicaciones.empresa AND art_ver_op_aplicaciones.tipoaplicacion = prodaplicaciones.codigo 
LEFT OUTER JOIN prodpantallas ON art_ver_op_aplicaciones.empresa = prodpantallas.empresa AND art_ver_op_aplicaciones.pantalla = prodpantallas.codigo 
LEFT OUTER JOIN prodformula ON art_ver_op_aplicaciones.empresa = prodformula.empresa AND art_ver_op_aplicaciones.formula = prodformula.formula
LEFT OUTER JOIN planificacion ON art_ver_op_aplicaciones.empresa = planificacion.empresa AND art_ver_op_aplicaciones.articulo = planificacion.cod_articulo 
   WHERE ( art_ver_op_aplicaciones.empresa = :empresa ) AND  
         ( art_ver_op_aplicaciones.articulo = :articulo ) AND  
         ( art_ver_op_aplicaciones.version = :version ) AND  
         ( art_ver_op_aplicaciones.operacion = :operacion ) and 
			( planificacion.ot = :ot )
ORDER BY art_ver_op_aplicaciones.empresa ASC,   
         art_ver_op_aplicaciones.articulo ASC,   
         art_ver_op_aplicaciones.version ASC,   
         art_ver_op_aplicaciones.orden ASC,   
         convert(integer, art_ver_op_aplicaciones.orden ) ASC   
" arguments=(("empresa", string),("articulo", string),("version", string),("operacion", string),("ot", number)) )
