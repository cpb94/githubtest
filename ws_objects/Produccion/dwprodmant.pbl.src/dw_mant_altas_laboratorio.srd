 retrieve="  SELECT articulos.empresa,   
         articulos.codigo,   
         articulos.descripcion,   
         articulos.pesopieza,   
         articulos.molde,   
         prodmoldes.descripcion,   
         articulos.marco,   
		prodmarcos.descripcion,
         articulos.costilla,   
		costilla.descripcion,
         articulos.plato_sup,   
         prodplatos_a.descripcion,   
         articulos.plato_inf,   
         prodplatos_b.descripcion,   
         articulos.codigo_pantallas,   
         articulos.bloqueado,   
         articulos.etiqueta_ce,   
         ce_marcado.descripcion,   
         articulos.referencia_laboratorio,   
         articulos.version_laboratorio,   
         articulos.formato,   
         articulos.modelo_origen,   
         articulos.pieza_origen,   
         articulos.relieve,   
         articulos.uso,   
         art_tipomoldura.descripcion,   
         articulos.venta_conjuntos,   
         articulos.espesor  
    FROM articulos 	LEFT OUTER JOIN prodplatos prodplatos_a ON articulos.empresa = prodplatos_a.empresa AND articulos.plato_sup = prodplatos_a.codigo 
							LEFT OUTER JOIN prodplatos prodplatos_b ON articulos.empresa = prodplatos_b.empresa AND articulos.plato_inf = prodplatos_b.codigo
							LEFT OUTER JOIN prodmoldes ON articulos.empresa = prodmoldes.empresa AND articulos.molde = prodmoldes.codigo 
							LEFT OUTER JOIN ce_marcado ON articulos.empresa = ce_marcado.empresa AND articulos.etiqueta_ce = ce_marcado.tipo AND articulos.cliente = ce_marcado.cliente 
							LEFT OUTER JOIN art_tipomoldura ON articulos.empresa = art_tipomoldura.empresa AND articulos.relieve = art_tipomoldura.codigo  
							LEFT OUTER JOIN prodmarcos ON articulos.empresa = prodmarcos.empresa AND articulos.marco = prodmarcos.codigo  
							LEFT OUTER JOIN costilla on articulos.costilla = costilla.codigo and articulos.empresa = costilla.empresa
   WHERE ( articulos.empresa = :cod_empresa ) AND  
         ( articulos.codigo = :codigo )    
" update="articulos" updatewhere=0 updatekeyinplace=yes arguments=(("cod_empresa", string),("codigo", string)) )
