boolean visible = false
integer x = 2418
integer y = 4
integer width = 878
integer height = 140
integer taborder = 70
string dragicon = "\bmp\arrastrar.ico"
boolean bringtotop = true
string dataobject = "dw_mant_prod_diaria_n"
boolean vscrollbar = true
