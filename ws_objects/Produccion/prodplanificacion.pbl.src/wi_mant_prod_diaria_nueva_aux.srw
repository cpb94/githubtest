event clicked;long maxi,i,num,donde,indice1,total1,posicion
dec  orden_produccion
int orden_anterior,orden_auxiliar,orden,anyo_orden
datetime fecha_grupo,fecha_auxiliar
string nueva_fecha,linea,sel

if not(bloqueando) then
	if not f_bloquear("proddiaria",codigo_empresa,"Mantenimiento Producción Diaria") then
      commit Using SQLCA;
		bloqueando = true
	else
		// problema concurrencia
		st_empresa.setfocus()
	end if
end if

if bloqueando then
	cb_aceptar.enabled    = true
	cb_cancelar.enabled   = true
	em_fechahasta.enabled = false
	em_fechadesde.enabled = false
	cb_listar.enabled     = false
	
	//choose case f_objeto_datawindow(this) 
	choose case dwo.name
		case "cb_anyadir" 
			if this.rowcount() = 0 then
				if isnull(date(em_nueva_fecha.text)) or year(date(em_nueva_fecha.text)) = 1900 then
					messagebox("Atención","Introduzca la fecha de la orden de producción")
					setfocus(em_nueva_fecha)
				else
					nueva_fecha = em_nueva_fecha.text
					donde = this.InsertRow(0)
					this.SetItem(donde,"empresa",codigo_empresa)
					this.SetItem(donde,"fecha", DateTime(Date(nueva_fecha)))
					this.SetItem(donde,"linea", linea_activa)
					this.SetItem(donde,"f_entrada", DateTime(today()))
					this.SetItem(donde,"anyo_orden_produccion",year(Date(nueva_fecha)))
					//this.SetItem(donde,"fabricado","N")
					this.SetItem(donde,"impresa","N")
					this.SetItem(donde,"orden",1)
					this.SetItem(donde,"hora_comienzo",DateTime(Date(nueva_fecha),time(0,0,0)))
					this.SetItem(donde,"respetar_hora_comienzo","N")
					this.groupcalc()
					dw_array[tab_1.SelectedTab].Setrow(donde)
					dw_array[tab_1.SelectedTab].Scrolltorow(donde)
					dw_array[tab_1.SelectedTab].SetColumn("orden_produccion")
				end if
			else			
				posicion = this.Find ("string(fecha,'dd/mm/yy')='"+string(date(em_nueva_fecha.text),"dd/mm/yy")+"' and linea = '"+linea_activa+"'", 1 , this.rowcount())
				if posicion < 1 then
					nueva_fecha = em_nueva_fecha.text
					donde = this.InsertRow(0)
					this.SetItem(donde,"empresa",codigo_empresa)
					this.SetItem(donde,"f_entrada",datetime(today()))				
					this.SetItem(donde,"fecha", DateTime(Date(nueva_fecha)))
					this.SetItem(donde,"linea", linea_activa)
					//this.SetItem(donde,"fabricado","N")
					this.SetItem(donde,"impresa","N")
					this.SetItem(donde,"orden",1)
					this.SetItem(donde,"anyo_orden_produccion",year(Date(nueva_fecha)))
					this.SetItem(donde,"hora_comienzo",DateTime(Date(nueva_fecha),time(0,0,0)))					
					this.SetItem(donde,"respetar_hora_comienzo","N")					
					this.groupcalc()
					dw_array[tab_1.SelectedTab].Setrow(donde)
					dw_array[tab_1.SelectedTab].Scrolltorow(donde)
					dw_array[tab_1.SelectedTab].SetColumn("orden_produccion")
				else		
					fecha_grupo = getitemdatetime(posicion,"fecha")
					indice1 = posicion + 1
					total1 = this.rowcount()
					if indice1 <= total1 then
						fecha_auxiliar = this.getitemdatetime(indice1,"fecha")
					end if
					do while indice1 <= total1 and fecha_grupo = fecha_auxiliar
						indice1 = indice1 +1
						if indice1 <= total1 then
							fecha_auxiliar = this.getitemdatetime(indice1,"fecha")
						end if
					loop
					orden_anterior = this.getitemnumber(indice1 -1,"orden")
					if isnull(orden_anterior) then orden_anterior = 0
					donde = this.InsertRow(indice1)
					this.SetItem(donde,"empresa",codigo_empresa)
					this.SetItem(donde,"f_entrada",datetime(today()))
					this.SetItem(donde,"linea", linea_activa)
					//this.SetItem(donde,"fabricado","N")
					this.SetItem(donde,"impresa","N")
					this.SetItem(donde,"fecha",fecha_grupo)
					this.SetItem(donde,"orden",orden_anterior + 1)
					this.SetItem(donde,"anyo_orden_produccion",year(Date(fecha_grupo)))
					this.SetItem(donde,"hora_comienzo",DateTime(Date(fecha_grupo),time(0,0,0)))
					this.SetItem(donde,"respetar_hora_comienzo","N")					
					this.groupcalc()
					dw_array[tab_1.SelectedTab].Setrow(donde)
					dw_array[tab_1.SelectedTab].scrolltorow(donde)
					dw_array[tab_1.SelectedTab].SetColumn("orden_produccion")
				end if
			end if	
		case "cb_intercalar"
			if row > 0 then
				donde = row
				fecha_grupo = getitemdatetime(donde,"fecha")
				orden = this.getitemnumber(donde,"orden")
				indice1 = donde
				total1 = this.rowcount()
				fecha_auxiliar = fecha_grupo
				do until indice1 > total1 or fecha_grupo <> fecha_auxiliar
					fecha_auxiliar = this.getitemdatetime(indice1,"fecha")
					orden_auxiliar = this.getitemnumber(indice1,"orden")
					this.setitem(indice1,"orden",orden_auxiliar + 1)
					indice1 = indice1 +1
				loop
				donde = this.InsertRow(donde)
				this.SetItem(donde,"empresa",codigo_empresa)
				this.SetItem(donde,"f_entrada",datetime(today()))
				this.SetItem(donde,"linea", linea_activa)
				//this.SetItem(donde,"fabricado","N")		
				this.SetItem(donde,"impresa","N")
				this.SetItem(donde,"fecha",fecha_grupo)
				this.SetItem(donde,"orden",orden)
				this.SetItem(donde,"anyo_orden_produccion",year(Date(fecha_grupo)))
				this.SetItem(donde,"hora_comienzo",DateTime(Date(fecha_grupo),time(0,0,0)))
				this.SetItem(donde,"respetar_hora_comienzo","N")					
				
				this.groupcalc()
				dw_array[tab_1.SelectedTab].setrow(donde)
				dw_array[tab_1.SelectedTab].Scrolltorow(donde)
				dw_array[tab_1.SelectedTab].SetColumn("orden_produccion")
			end if
		case "cb_borrar"
				if this.RowCount() > 0 then
					fecha_grupo = this.getitemdatetime(row,"fecha")
					indice1 = row
					total1 = this.rowcount()
					fecha_auxiliar = fecha_grupo
					do until indice1 > total1 or fecha_grupo <> fecha_auxiliar
						fecha_auxiliar = this.getitemdatetime(indice1,"fecha")
						orden_auxiliar = this.getitemnumber(indice1,"orden")
						this.setitem(indice1,"orden",orden_auxiliar - 1)
						indice1 = indice1 +1
					loop
					indice1 = row
					this.DeleteRow(indice1)
					if indice1 > 1 then
						dw_array[tab_1.SelectedTab].Setrow(indice1 -1)
					end if
				end if
		case "pb_numero_orden"		
			orden_produccion = this.object.orden_produccion[this.getrow()]
			if isnull(orden_produccion) then orden_produccion = 0
			if orden_produccion = 0 then
				linea      = this.object.linea[this.getrow()]
				anyo_orden = this.object.anyo_orden_produccion[this.getrow()]
				this.object.orden_produccion[this.getrow()] = f_numero_orden_linea(codigo_empresa,linea,anyo_orden)
				this.accepttext()
			end if	
		case else
			if row > 0 then
				this.setrow(row)
				drag(begin!)
			end if			
	end choose
	
	call super::clicked
end if
end event

