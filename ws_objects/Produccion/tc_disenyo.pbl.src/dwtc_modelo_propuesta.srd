 retrieve="  SELECT  modelo_propuesta.empresa ,           
modelo_propuesta.modelo ,           
modelo_propuesta.cliente ,           
modelo_propuesta.codigo ,           
modelo_propuesta.formato ,           
formatos.abreviado ,           
formatos.largo ,           
formatos.ancho ,           
modelo_propuesta.numero_piezas ,          
 modelo_propuesta.formato as formato_actual ,           
modelo_propuesta.numero_piezas as npiezas_actual ,           
(SELECT COUNT(*) FROM modelo_propuesta_sistema_archivos A WHERE A.empresa = modelo_propuesta.empresa AND A.modelo = modelo_propuesta.modelo AND A.cliente = modelo_propuesta.cliente  AND A.propuesta = modelo_propuesta.codigo) AS ficheros,           
0 as seleccionado,          
 modelo_propuesta.rechazado     
FROM modelo_propuesta 
LEFT OUTER JOIN formatos ON modelo_propuesta.empresa = formatos.empresa AND modelo_propuesta.formato = formatos.codigo 
WHERE modelo_propuesta.empresa = :empresa 
AND modelo = :modelo" update="modelo_propuesta" updatewhere=0 updatekeyinplace=no arguments=(("empresa", string),("modelo", string))  sort="integer(modelo_propuesta_codigo) A " )
