 retrieve="  SELECT art_tipomoldura.codigo,   
         art_tipomoldura.descripcion,   
         REPLACE((SELECT ruta_archivos_relieve FROM empresas WHERE empresa = '1') + art_tipomoldura.ruta+ (SELECT tamanyo FROM archivo_resolucion WHERE empresa = '1' AND codigo = '5')+'\'+art_tipomoldura.fichero, ' ','') AS ruta_imagen,  
  		 art_tipomoldura.cliente,
		genter.razon
    FROM art_tipomoldura LEFT OUTER JOIN genter ON art_tipomoldura.empresa = genter.empresa AND art_tipomoldura.cliente = genter.codigo  AND genter.tipoter = 'C'
"  sort="art_tipomoldura_descripcion A " )
