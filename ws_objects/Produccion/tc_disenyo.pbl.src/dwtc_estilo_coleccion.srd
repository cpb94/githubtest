table(column=(type=char(5) updatewhereclause=yes name=estilo_empresa dbname="estilo.empresa" )
 column=(type=char(5) updatewhereclause=yes name=estilo_codigo dbname="estilo.codigo" )
 column=(type=char(50) updatewhereclause=yes name=estilo_descripcion dbname="estilo.descripcion" )
 column=(type=long updatewhereclause=yes name=seleccionado dbname="seleccionado" )
 retrieve=" SELECT estilo.empresa, 
         estilo.codigo,  
         estilo.descripcion,
         1 AS seleccionado 
    FROM estilo 
   WHERE estilo.empresa = :empresa AND estilo.codigo IN(SELECT coleccion_estilo.estilo FROM coleccion_estilo WHERE coleccion_estilo.empresa = estilo.empresa AND coleccion_estilo.coleccion = :coleccion) 
   UNION
  SELECT estilo.empresa, 
         estilo.codigo,  
         estilo.descripcion,
         0 AS seleccionado 
    FROM estilo 
   WHERE estilo.empresa = :empresa AND estilo.codigo NOT IN(SELECT coleccion_estilo.estilo FROM coleccion_estilo WHERE coleccion_estilo.empresa = estilo.empresa AND coleccion_estilo.coleccion = :coleccion)
" arguments=(("empresa", string),("coleccion", string))  sort="integer(estilo_codigo) A " )
