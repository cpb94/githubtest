 retrieve="  SELECT art_tipomoldura.codigo,   
         art_tipomoldura.empresa,   
         art_tipomoldura.descripcion,   
         art_tipomoldura.ruta,   
         art_tipomoldura.fichero,   
          REPLACE((SELECT ruta_archivos_relieve FROM empresas WHERE empresa = '1') + ruta+ (SELECT tamanyo FROM archivo_resolucion WHERE empresa = '1' AND codigo = '1')+'\'+fichero, ' ','') AS ruta_imagen,   
         art_tipomoldura.cliente,
		genter.razon
    FROM art_tipomoldura LEFT OUTER JOIN genter ON art_tipomoldura.empresa = genter.empresa AND art_tipomoldura.cliente = genter.codigo  AND genter.tipoter = 'C'
   WHERE ( art_tipomoldura.empresa = :empresa ) AND  
         ( art_tipomoldura.codigo = :codigo )    
" update="art_tipomoldura" updatewhere=0 updatekeyinplace=no arguments=(("empresa", string),("codigo", string)) )
