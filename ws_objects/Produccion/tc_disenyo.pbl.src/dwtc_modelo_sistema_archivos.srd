 retrieve="  SELECT modelo_sistema_archivos.modelo,   
         modelo_sistema_archivos.codigo,   
         modelo_sistema_archivos.empresa,   
         modelo_sistema_archivos.ruta,   
         modelo_sistema_archivos.fichero,   
         modelo_sistema_archivos.observaciones,   
         modelo_sistema_archivos.cliente,   
         genter.razon,   
         0 as ruta_local,   
         '' as ruta_antigua,   
         '' as archivo_local,
			'' as cliente_actual  
    FROM modelo_sistema_archivos LEFT OUTER JOIN genter ON modelo_sistema_archivos.empresa = genter.empresa AND modelo_sistema_archivos.cliente = genter.codigo AND genter.tipoter = 'C' 
   WHERE ( modelo_sistema_archivos.empresa = :empresa ) AND  
         ( modelo_sistema_archivos.modelo = :codigo )    
" update="modelo_sistema_archivos" updatewhere=0 updatekeyinplace=no arguments=(("empresa", string),("codigo", string)) )
