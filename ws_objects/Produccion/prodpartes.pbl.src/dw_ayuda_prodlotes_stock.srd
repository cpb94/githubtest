table(column=(type=char(20) updatewhereclause=yes name=lote dbname="lote" )
 column=(type=char(20) updatewhereclause=yes name=articulo dbname="articulo" )
 column=(type=char(5) updatewhereclause=yes name=empresa dbname="empresa" )
 column=(type=datetime updatewhereclause=yes name=fecha_max dbname="fecha_max" )
 retrieve="
select partes.lote, partes.articulo, partes.empresa, movi.fecha_max
from  (select lote, articulo, empresa
	   from prodpartes_ot
	   where empresa = :empresa
	   and lote <> ''
	   and ot_terminada <> 'N'
	   and ajuste <> 'S'
	   group by lote, articulo, empresa) as partes
left join (select max(fecha_mov) as fecha_max, articulo, tono, empresa
			from almacenmovimientos
			where empresa = '1'
			and tipomov = 209
			group by articulo,tono,empresa) as movi 
			on movi.articulo = partes.articulo 
			and movi.tono = partes.lote
			and movi.empresa = partes.empresa
" arguments=(("empresa", string),("criterio", string)) )
