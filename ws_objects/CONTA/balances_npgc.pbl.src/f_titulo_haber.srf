global function string f_titulo_haber (string emp, string codbal);string retorno

  SELECT titulo_haber INTO :retorno  FROM contabalnpgc
   WHERE ( empresa = :emp ) AND  
			( balance = :codbal );

retorno = trim(retorno)         
			
if isnull(retorno) then retorno=""

return retorno
end function

