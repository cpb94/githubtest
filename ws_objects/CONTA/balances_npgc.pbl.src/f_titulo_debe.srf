global function string f_titulo_debe (string emp, string codbal);string retorno

  SELECT titulo_debe INTO :retorno  FROM contabalnpgc
   WHERE ( empresa = :emp ) AND  
			( balance = :codbal );

retorno = trim(retorno)         
			
if isnull(retorno) then retorno=""

return retorno
end function

