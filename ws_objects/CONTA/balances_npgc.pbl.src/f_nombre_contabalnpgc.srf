global function string f_nombre_contabalnpgc (string emp, string codbal);string retorno

  SELECT titulo INTO :retorno  FROM contabalnpgc
   WHERE ( empresa = :emp ) AND  
			( balance = :codbal );
         
if isnull(retorno) then retorno=""

return retorno
end function

