global function string f_coste_contagene (string empresa, string codigo);string coste

coste = ""

SELECT contagene.centro_coste INTO :coste FROM contagene    
WHERE  contagene.empresa = :empresa
AND    contagene.codigo  = :codigo    ;

return coste
end function

