global function string f_dh_contagene (string empresa, string codigo);string dh

dh = ""

SELECT contagene.dh INTO :dh FROM contagene    
WHERE  contagene.empresa = :empresa
AND    contagene.codigo  = :codigo    ;

return dh
end function

