global function string f_tipo_contalibros (string codigo);String var_descripcion

CHOOSE CASE codigo
	CASE "C"
	   var_descripcion ="COMPRAS"
   CASE "V"
	   var_descripcion ="VENTAS"
END CHOOSE



Return var_descripcion
end function

