 retrieve="  SELECT  contaconcepto.resumido ,           costesapu.ejercicio ,           costesapu.empresa ,           costesapu.mes ,           costesapu.origen ,           costesapu.asiento ,           costesapu.apunte ,           costesapu.coste ,           costesapu.debe ,           costesapu.haber ,           costesapu.concepto ,           costesapu.ampliacion ,           costesapu.cuenta ,           costesapu.fapunte     
        FROM contaconcepto ,           costesapu     
        WHERE 	( costesapu.empresa *= contaconcepto.empresa) and          ( costesapu.concepto *= contaconcepto.codigo) and          ( ( costesapu.empresa = :arg_empresa ) and          ( costesapu.mes between :arg_perid and :arg_perih ) and          ( costesapu.ejercicio = :arg_ejer ) )  " arguments=(("arg_empresa", string),("arg_ejer", number),("arg_perid", number),("arg_perih", number)) )
