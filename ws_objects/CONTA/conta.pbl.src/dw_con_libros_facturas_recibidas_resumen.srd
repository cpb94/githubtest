table(column=(type=char(10) updatewhereclause=yes name=tipoiva dbname="contaapun.tipoiva" dbalias=".tipoiva" )
 column=(type=decimal(2) updatewhereclause=yes identity=yes name=debe dbname="debe" )
 column=(type=decimal(2) updatewhereclause=yes identity=yes name=haber dbname="haber" )
 column=(type=decimal(4) updatewhereclause=yes identity=yes name=imponible dbname="imponible" )
 column=(type=char(10) updatewhereclause=yes name=tipoapu dbname="contaapun.tipoapu" dbalias=".tipoapu" )
 retrieve="  SELECT contaapun.tipoiva,   
         Sum(debe) debe,Sum(haber) haber,sum(imponible) imponible,   
         contaapun.tipoapu  
    FROM contaapun  
   WHERE ( contaapun.ejercicio = :ejercicio ) AND  
         ( contaapun.empresa = :empresa ) AND  
         (contaapun.tipoapu = ~"2~" OR  
         contaapun.tipoapu = ~"7~" OR  
         contaapun.tipoapu = ~"9~"OR  
         contaapun.tipoapu = ~"10~") AND  
         contaapun.fregistro between :fecha_desde and :fecha_hasta   
GROUP BY contaapun.tipoapu,   
         contaapun.tipoiva   
" arguments=(("ejercicio", number),("empresa", string),("fecha_desde", datetime),("fecha_hasta", datetime)) )
