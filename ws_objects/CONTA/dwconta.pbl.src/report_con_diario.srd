 retrieve="  SELECT contaapun.ejercicio,   
         contaapun.empresa,   
         contaapun.asiento,   
         contaapun.apunte,   
         contaapun.cuenta,   
         contaapun.fapunte,   
         contaapun.diario,   
         contaapun.concepto,   
         contaapun.ampliacion,   
         contaapun.debe,   
         contaapun.haber,   
         contaapun.tipoapu,   
         contaapun.documento,   
         contaapun.factura,   
         contaapun.ffactura,   
         contaapun.imponible,   
         contaapun.tipoiva,   
         contaapun.iva,   
         contaapun.divisas,   
         contaapun.deducible,   
         contaapun.actualizado,   
         contaapun.listado,   
         contaapun.registro,   
         contaapun.origen,   
         contaapun.tipoter,   
         contaapun.clipro,   
         contaapun.conciliado,   
         contaapun.nif,   
         contaapun.punteado,   
         contaapun.fvalor,   
         contaapun.docpunteo,   
         contaapun.coste,   
         contaapun.fregistro,   
         contaapun.cambio,   
         contaapun.moneda,   
         contaapun.mes,   
         0 cont  
    FROM contaapun  
   WHERE ( contaapun.empresa = :arg_empresa ) AND  
         ( contaapun.ejercicio = :arg_ejer ) AND  
         ( contaapun.mes between :arg_perid and :arg_perih ) AND  
         ( contaapun.asiento between :arg_asid and :arg_asih )   
ORDER BY contaapun.empresa ASC,   
         contaapun.ejercicio ASC,   
         contaapun.mes ASC,   
         contaapun.fapunte ASC,   
         contaapun.asiento ASC,   
         contaapun.apunte ASC   
" update="contaapun" updatewhere=0 updatekeyinplace=no arguments=(("arg_empresa", string),("arg_ejer", number),("arg_perid", number),("arg_perih", number),("arg_asid", number),("arg_asih", number),("arg_pag_inicial", number)) )
