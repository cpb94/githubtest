 retrieve="  SELECT convert(char(19),substring(contaapun.cuenta,1,:arg_dig1)) nivel1,
         convert(char(20),substring(contaapun.cuenta,1,:arg_dig2)) nivel2,   
         convert(char(19),substring(contaapun.cuenta,1,:arg_dig3)) nivel3,
         convert(char(19),substring(contaapun.cuenta,1,:arg_dig4)) nivel4,
         convert(char(19),substring(contaapun.cuenta,1,:arg_dig5)) nivel5,          
		  	contaapun.fapunte,
         sum(contaapun.debe) debe,
         sum(contaapun.haber) haber
    FROM contaapun  
   WHERE ( contaapun.ejercicio = :arg_ejer ) AND  
         ( contaapun.empresa = :arg_empresa ) AND  
         ( contaapun.cuenta between :cuenta_desde and :cuenta_hasta )
  GROUP BY convert(char(19),substring(contaapun.cuenta,1,:arg_dig1)),
         	convert(char(20),substring(contaapun.cuenta,1,:arg_dig2)),   
         convert(char(19),substring(contaapun.cuenta,1,:arg_dig3)) ,
         convert(char(19),substring(contaapun.cuenta,1,:arg_dig4)),
         convert(char(19),substring(contaapun.cuenta,1,:arg_dig5)) , 
		  	contaapun.fapunte



" retrieve.asneeded=yes arguments=(("arg_ejer", number),("arg_empresa", string),("arg_dig1", number),("arg_dig2", number),("arg_dig3", number),("arg_dig4", number),("arg_dig5", number),("arg_desde", datetime),("arg_hasta", datetime),("arg_desde_ejer", datetime),("arg_hasta_ejer", datetime),("cuenta_desde", string),("cuenta_hasta", string)) )
