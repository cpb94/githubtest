global function string f_incidencias_uti_parametros (string codigo);String var
  SELECT uti_parametros.incidencias 
    INTO :var  
    FROM uti_parametros  
   WHERE uti_parametros.empresa = :codigo   ;
	IF SQLCA.SQLCODE = 100 Then var = "S"
	
	Return var

end function

