global function string f_busquedas_uti_parametros (string codigo);String var
  SELECT uti_parametros.busquedas  
    INTO :var  
    FROM uti_parametros  
   WHERE uti_parametros.empresa = :codigo   ;
	IF SQLCA.SQLCODE = 100 Then var = "1"
	
	Return var

end function

