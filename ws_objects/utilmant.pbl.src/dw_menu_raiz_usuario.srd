 retrieve="  SELECT uti_menu.codigo,   
         uti_menu.descripcion,   
         uti_menu.menu,   
         uti_menu.libreria, 
			uti_menu.empresa,
			uti_permisos_menu.menu,
			uti_permisos_menu.usuario,
			uti_permisos_menu.empresa,
         uti_permisos_menu.permiso,
			uti_menu.orden,
			:permisos as mostrar_permisos,
			0 as resaltado,
			0 as seleccionado
    FROM uti_menu   
    LEFT OUTER JOIN uti_permisos_menu ON uti_permisos_menu.empresa = uti_menu.empresa AND uti_permisos_menu.menu = uti_menu.codigo   
	 WHERE 
			uti_menu.empresa = :empresa AND
			uti_menu.elemento_padre = 0 AND
			uti_permisos_menu.usuario = :usuario AND
			uti_permisos_menu.permiso = 1
	 ORDER BY orden ASC" arguments=(("usuario", string),("permisos", number),("empresa", string)) )
