 retrieve="  SELECT venmprima.envase,   
         venmprima.tipo_materia,   
         ~"               ~" codigo_familia,   
         venmprima.familia,   
         comartpro.proveedor,   
         comartpro.precio,   
         venmprima.almacen_entra,   
         venmprima.control_almacen,   
         venmprima.cuenta,   
         venmprima.codigo,   
         venmprima.tipo_unidad,   
         comproveedores.decimales_precios,   
         venmprima.descripcion  
    FROM comproveedores RIGHT OUTER JOIN comartpro ON comproveedores.empresa = comartpro.empresa AND comproveedores.codigo = comartpro.proveedor,   
         venmprima  
   WHERE ( comartpro.empresa = venmprima.empresa ) and  
         ( comartpro.mprima = venmprima.codigo ) and  
         ( ( venmprima.empresa = :empresa ) AND  
         ( comartpro.proveedor like :arg_proveedor ) AND  
         ( venmprima.tipo_materia in ( :arg_tipos_materias ) ) )   and
			venmprima.codigo in ( 	select mprima
											from comartpro
											WHERE empresa = :empresa  
											group by mprima
											having count(*) >= :num_proveedores )
											" arguments=(("empresa", string),("arg_proveedor", string),("arg_tipos_materias", stringlist),("num_proveedores", number))  sort="venmprima_tipo_materia A venmprima_familia A venmprima_descripcion A " )
