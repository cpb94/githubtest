global function string f_seccion_mprima (string empresa, string codigo);String seccion

SELECT seccion 
INTO   :seccion
FROM   venmprima  
WHERE  empresa = :codigo_empresa
AND    codigo  = :codigo ;

if isnull(seccion) then seccion = ""

return seccion
end function

