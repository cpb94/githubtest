global function string f_nombre_formaenvio (string empresa, string codigo);  string nombre
  SELECT comforenv.descripcion  
    INTO :nombre  
    FROM comforenv  
   WHERE ( comforenv.empresa = :empresa ) AND  
         ( comforenv.codigo = :codigo )   ;
	return nombre

end function

