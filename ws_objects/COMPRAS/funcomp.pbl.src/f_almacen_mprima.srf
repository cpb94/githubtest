global function string f_almacen_mprima (string empresa, string codigo);String almacen

SELECT almacen_entra 
INTO   :almacen 
FROM   venmprima  
WHERE  empresa = :codigo_empresa
AND    codigo  = :codigo ;

IF IsNull(almacen ) THEN almacen = ""

return almacen
end function

