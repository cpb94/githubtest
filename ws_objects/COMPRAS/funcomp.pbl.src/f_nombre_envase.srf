global function string f_nombre_envase (string codigo); string nombre_envase
 SELECT venenvases.descripcion  
    INTO :nombre_envase  
    FROM venenvases  
   WHERE ( venenvases.empresa = :codigo_empresa ) AND  
         ( venenvases.envase = :codigo )   ;
  return nombre_envase
end function

