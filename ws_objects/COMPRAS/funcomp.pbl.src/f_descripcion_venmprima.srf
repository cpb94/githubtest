global function string f_descripcion_venmprima (string empresa, string mprima);String descrip
  SELECT venmprima.descripcion 
  INTO :descrip
  FROM venmprima  
  WHERE venmprima.empresa = :empresa  
	AND   venmprima.codigo  = :mprima   ;

Return descrip
end function

