 retrieve="PBSELECT( VERSION(400) TABLE(NAME=~"venmprima~" )  TABLE(NAME=~"venmateria~" )  TABLE(NAME=~"venfamilias~" ) COLUMN(NAME=~"venmprima.empresa~") COLUMN(NAME=~"venmprima.codigo~") COLUMN(NAME=~"venmprima.descripcion~") COLUMN(NAME=~"venmprima.tipo_materia~") COLUMN(NAME=~"venmprima.proveedor~") COLUMN(NAME=~"venmprima.familia~") COLUMN(NAME=~"venmprima.tipo_unidad~") COLUMN(NAME=~"venmprima.control_almacen~") COLUMN(NAME=~"venmprima.envase~") COLUMN(NAME=~"venmprima.cuenta~") COLUMN(NAME=~"venfamilias.cuenta~") COLUMN(NAME=~"venmateria.cuenta~") COLUMN(NAME=~"venmprima.precio_coste~")    JOIN (LEFT=~"venmprima.empresa~"    OP =~"=~"RIGHT=~"venmateria.empresa~"    OUTER1 =~"venmprima.empresa~" )    JOIN (LEFT=~"venmprima.tipo_materia~"    OP =~"=~"RIGHT=~"venmateria.codigo~"    OUTER1 =~"venmprima.tipo_materia~" )    JOIN (LEFT=~"venmprima.empresa~"    OP =~"=~"RIGHT=~"venfamilias.empresa~"    OUTER1 =~"venmprima.empresa~" )    JOIN (LEFT=~"venmprima.tipo_materia~"    OP =~"=~"RIGHT=~"venfamilias.tipo_materia~"    OUTER1 =~"venmprima.tipo_materia~" )    JOIN (LEFT=~"venmprima.familia~"    OP =~"=~"RIGHT=~"venfamilias.familia~"    OUTER1 =~"venmprima.familia~" )WHERE(    EXP1 =~"venmprima.empresa~"   OP =~"=~"    EXP2 =~":empresa~"    LOGIC =~"and~" ) WHERE(    EXP1 =~"venmprima.tipo_materia~"   OP =~"between~"    EXP2 =~":prodd and :prodh~" ) ) ORDER(NAME=~"venmprima.empresa~" ASC=yes )  ORDER(NAME=~"venmprima.tipo_materia~" ASC=yes )  ORDER(NAME=~"venmprima.familia~" ASC=yes )  ORDER(NAME=~"venmprima.proveedor~" ASC=yes )  ORDER(NAME=~"venmprima.descripcion~" ASC=yes ) ARG(NAME = ~"empresa~" TYPE = string)  ARG(NAME = ~"prodd~" TYPE = string)  ARG(NAME = ~"prodh~" TYPE = string) " arguments=(("empresa", string),("prodd", string),("prodh", string)) )
