 retrieve="  SELECT almacenmovimientos.empresa, 
		almacenmovimientos.almacen,   
         almacenmovimientos.zona,   
         almacenmovimientos.fila,   
         almacenmovimientos.altura,   
         almacenmovimientos.articulo, 
		articulos.descripcion,
		almacenmovimientos.calidad,
		calidades.abreviado,
         almacenmovimientos.calibre,               
         almacenmovimientos.tono,
		(SUM (almacenmovimientos.cantidade - almacenmovimientos.cantidads) 
		 - 
		 ISNULL(
				(SELECT SUM(orden_carga_linea_origen.cantidad)
				FROM orden_carga_linea_origen 
				INNER JOIN orden_carga ON orden_carga_linea_origen.empresa = orden_carga.empresa AND orden_carga_linea_origen.anyo = orden_carga.anyo AND orden_carga_linea_origen.codigo = orden_carga.codigo   
				INNER JOIN orden_carga_linea ON orden_carga_linea.empresa = orden_carga_linea_origen.empresa AND orden_carga_linea.anyo = orden_carga_linea_origen.anyo AND orden_carga_linea.codigo = orden_carga_linea_origen.codigo AND orden_carga_linea.linea = orden_carga_linea_origen.linea
				INNER JOIN venliped ON orden_carga_linea.empresa = venliped.empresa AND orden_carga_linea.venliped_anyo = venliped.anyo AND orden_carga_linea.venliped_pedido = venliped.pedido AND orden_carga_linea.venliped_linea = venliped.linea 
				WHERE orden_carga.finalizada = 'N' 
				AND venliped.articulo = almacenmovimientos.articulo  
				AND orden_carga_linea_origen.empresa = almacenmovimientos.empresa 
				AND orden_carga_linea_origen.almacen = almacenmovimientos.almacen 
				AND orden_carga_linea_origen.zona = almacenmovimientos.zona 
				AND orden_carga_linea_origen.fila = almacenmovimientos.fila 
				AND orden_carga_linea_origen.altura = almacenmovimientos.altura 
				AND venliped.calidad = almacenmovimientos.calidad 
				AND venliped.tonochar = almacenmovimientos.tono
				AND venliped.calibre = almacenmovimientos.calibre
				GROUP BY  venliped.articulo)
				, 0
				)
		 ) as existencias,
		0 as seleccionada,
		0.0000 as cantidad, 
		almacenmovimientos_observaciones.observaciones, 
		articulos.unidad,
		unidades.abreviado,
		almacenmovimientos.caja,
		almcajas.descripcion_abr,
		almartcajas.metroscaja, 
		almartcajas.piezascaja,
		articulos.decimales_unidad,
		almacenmovimientos.tipo_pallet,
		almacenmovimientos.formato
    FROM almacenmovimientos  
INNER JOIN calidades ON almacenmovimientos.empresa = calidades.empresa AND almacenmovimientos.calidad = calidades.codigo
INNER JOIN articulos ON almacenmovimientos.empresa = articulos.empresa AND almacenmovimientos.articulo = articulos.codigo 
LEFT OUTER JOIN almacenmovimientos_observaciones ON almacenmovimientos.empresa = almacenmovimientos_observaciones.empresa AND almacenmovimientos.almacen = almacenmovimientos_observaciones.almacen AND almacenmovimientos.zona = almacenmovimientos_observaciones.zona AND almacenmovimientos.fila = almacenmovimientos_observaciones.fila AND almacenmovimientos.altura = almacenmovimientos_observaciones.altura AND almacenmovimientos.articulo = almacenmovimientos_observaciones.articulo AND almacenmovimientos.calidad = almacenmovimientos_observaciones.calidad AND almacenmovimientos.tono = almacenmovimientos_observaciones.tono AND almacenmovimientos.calibre = almacenmovimientos_observaciones.calibre AND almacenmovimientos.tipo_pallet = almacenmovimientos_observaciones.tipo_pallet AND almacenmovimientos.caja = almacenmovimientos_observaciones.caja
LEFT OUTER JOIN almartcajas ON almartcajas.empresa = almacenmovimientos.empresa AND almartcajas.articulo = almacenmovimientos.articulo AND almartcajas.caja = almacenmovimientos.caja
LEFT OUTER JOIN almcajas ON almcajas.empresa = almacenmovimientos.empresa AND almcajas.codigo = almacenmovimientos.caja
LEFT OUTER JOIN unidades ON articulos.unidad = unidades.codigo 
   WHERE ( almacenmovimientos.empresa = :empresa ) 
   AND  ( almacenmovimientos.articulo = :articulo )    
   AND  ( almacenmovimientos.calidad = :calidad )    
   AND  ( almacenmovimientos.tono = :tono )    
   AND  ( almacenmovimientos.calibre = :calibre )    
   AND  ( almacenmovimientos.tipo_pallet = :tipo_pallet )    
   AND  ( almacenmovimientos.caja = :caja )    
   AND (almacenmovimientos.formato = :formato)
   AND not  ( almacenmovimientos.almacen = ~"1~" and almacenmovimientos.zona = ~"P~" )    
GROUP BY almacenmovimientos.empresa, 
		almacenmovimientos.almacen,   
         almacenmovimientos.zona,   
         almacenmovimientos.fila,   
         almacenmovimientos.altura,   
         almacenmovimientos.articulo,   
		articulos.descripcion,
		almacenmovimientos.calidad,
		calidades.abreviado,
         almacenmovimientos.calibre,               
         almacenmovimientos.tono, 
		almacenmovimientos_observaciones.observaciones,
		articulos.unidad,
		unidades.abreviado,
		almacenmovimientos.caja,
		almcajas.descripcion_abr,
		almartcajas.metroscaja, 
		almartcajas.piezascaja,
		articulos.decimales_unidad,
		almacenmovimientos.tipo_pallet,
		almacenmovimientos.formato
/*
HAVING (SUM (almacenmovimientos.cantidade - almacenmovimientos.cantidads) 
		 - 
		 ISNULL(
				(SELECT SUM(orden_carga_linea_origen.cantidad)
				FROM orden_carga_linea_origen 
				INNER JOIN orden_carga ON orden_carga_linea_origen.empresa = orden_carga.empresa AND orden_carga_linea_origen.anyo = orden_carga.anyo AND orden_carga_linea_origen.codigo = orden_carga.codigo   
				INNER JOIN orden_carga_linea ON orden_carga_linea.empresa = orden_carga_linea_origen.empresa AND orden_carga_linea.anyo = orden_carga_linea_origen.anyo AND orden_carga_linea.codigo = orden_carga_linea_origen.codigo AND orden_carga_linea.linea = orden_carga_linea_origen.linea
				INNER JOIN venliped ON orden_carga_linea.empresa = venliped.empresa AND orden_carga_linea.venliped_anyo = venliped.anyo AND orden_carga_linea.venliped_pedido = venliped.pedido AND orden_carga_linea.venliped_linea = venliped.linea 
				WHERE orden_carga.finalizada = 'N' AND venliped.articulo = almacenmovimientos.articulo  AND orden_carga_linea_origen.empresa = almacenmovimientos.empresa AND orden_carga_linea_origen.almacen = almacenmovimientos.almacen AND orden_carga_linea_origen.zona = almacenmovimientos.zona AND orden_carga_linea_origen.fila = almacenmovimientos.fila AND orden_carga_linea_origen.altura = almacenmovimientos.altura
				GROUP BY  venliped.articulo)
				, 0
				)
		 )  > 0
*/" arguments=(("empresa", string),("articulo", string),("calidad", string),("tono", string),("calibre", number),("unidad", string),("tipo_pallet", string),("caja", string),("formato", string))  sort="existencias D " )
