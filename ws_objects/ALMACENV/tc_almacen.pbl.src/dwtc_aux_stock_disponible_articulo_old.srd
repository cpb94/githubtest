 column=(type=char(5) updatewhereclause=yes name=almacenmovimientos_caja dbname="almacenmovimientos.caja" )
 column=(type=char(10) updatewhereclause=yes name=almcajas_descripcion_abr dbname="almcajas.descripcion_abr" )
 column=(type=decimal(4) updatewhereclause=yes name=almartcajas_metroscaja dbname="almartcajas.metroscaja" )
 column=(type=decimal(4) updatewhereclause=yes name=almartcajas_piezascaja dbname="almartcajas.piezascaja" )
