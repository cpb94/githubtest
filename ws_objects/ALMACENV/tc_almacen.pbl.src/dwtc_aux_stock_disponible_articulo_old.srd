 column=(type=char(20) updatewhereclause=yes name=tono dbname="almacenmovimientos.tono" )
 column=(type=decimal(6) updatewhereclause=yes identity=yes name=existencias dbname="existencias" )
 column=(type=long updatewhereclause=yes identity=yes name=seleccionada dbname="seleccionada" values="	1/	0" )
 column=(type=decimal(4) updatewhereclause=yes identity=yes name=cantidad dbname="cantidad" )
