 retrieve="SELECT sum(almacenmovimientos.cantidade - almacenmovimientos.cantidads) as stock,   
        ISNULL((SELECT sum(CASE WHEN venliped.tipo_unidad = '0' THEN venliped.cantidad ELSE (floor(venliped.cantidad / av2.metroscaja) * av2.piezascaja) END) 
			FROM venliped 
			INNER JOIN venped ON venliped.empresa = venped.empresa AND venliped.anyo = venped.anyo AND venliped.pedido = venped.pedido 
			LEFT OUTER JOIN almartcajas av2 ON venliped.empresa = av2.empresa AND venliped.articulo = av2.articulo AND venliped.caja = av2.caja
			WHERE av2.piezascaja <> 0 and venliped.empresa = almacenmovimientos.empresa 
			AND venliped.articulo = almacenmovimientos.articulo 
			AND venliped.calidad = almacenmovimientos.calidad 
			AND venliped.tonochar = almacenmovimientos.tono 
			AND venliped.calibre = almacenmovimientos.calibre 
			AND venped.es_proforma = 'N'
		),0) as pedidos,   
         almacenmovimientos.empresa,   
         almacenmovimientos.articulo,   
         almacenmovimientos.calidad,   
         almacenmovimientos.tono,   
         almacenmovimientos.calibre,
		almacenmovimientos.caja, 
		av.piezascaja,
		av.metroscaja,
		articulos.unidad,
		articulos.decimales_unidad,
		almacenmovimientos.tipo_pallet    
    FROM almacenmovimientos  
LEFT OUTER JOIN almartcajas av ON almacenmovimientos.empresa = av.empresa AND almacenmovimientos.articulo = av.articulo AND almacenmovimientos.caja = av.caja 
LEFT OUTER JOIN articulos ON almacenmovimientos.empresa = articulos.empresa AND almacenmovimientos.articulo = articulos.codigo
   WHERE ( almacenmovimientos.empresa = :empresa ) AND  
         ( almacenmovimientos.articulo = :articulo )   
GROUP BY almacenmovimientos.empresa,   
         almacenmovimientos.articulo,   
         almacenmovimientos.calidad,   
         almacenmovimientos.tono,   
         almacenmovimientos.calibre, 
		almacenmovimientos.caja, 
		av.piezascaja,
		av.metroscaja,
		articulos.unidad,
		articulos.decimales_unidad,
		almacenmovimientos.tipo_pallet           
HAVING  sum(almacenmovimientos.cantidade - almacenmovimientos.cantidads) > 0" arguments=(("empresa", string),("articulo", string)) )
