 column=(type=char(30) updatewhereclause=yes name=formatos_descripcion dbname="formatos.descripcion" dbalias=".descripcion" )
 column=(type=decimal(4) updatewhereclause=yes name=almartcajas_piezascaja dbname="almartcajas.piezascaja" dbalias=".piezascaja" )
 column=(type=decimal(4) updatewhereclause=yes name=almartcajas_metroscaja dbname="almartcajas.metroscaja" dbalias=".metroscaja" )
 column=(type=char(5) updatewhereclause=yes name=articulos_unidad dbname="articulos_unidad" )
 column=(type=long updatewhereclause=yes identity=yes name=articulos_decimales_unidad dbname="articulos_decimales_unidad" )
 column=(type=decimal(0) updatewhereclause=yes identity=yes name=alm_bultos_lineas_cantidad dbname="alm_bultos_lineas_cantidad" )
 column=(type=char(20) updatewhereclause=yes name=formatos_abreviado dbname="formatos.abreviado" dbalias=".abreviado" )
 retrieve="  SELECT articulos.descripcion,   
         formatos.descripcion,   
         almartcajas.piezascaja,   
         almartcajas.metroscaja,   
         case when ventipolin.gestionar_en_piezas = 'S' then '0' else articulos.unidad end as articulos_unidad,   
         case when ventipolin.gestionar_en_piezas = 'S' then 0 else articulos.decimales_unidad end as articulos_decimales_unidad,   
         convert(decimal(16,0),( case when (case when ventipolin.gestionar_en_piezas = 'S' then '0' else articulos.unidad end) <> '1' then isnull((case when ventipolin.gestionar_en_piezas = 'S' then venliped.cantidad_pzs else venliped.cantidad end),0) else (isnull(venliped.cantidad,0) / almartcajas.metroscaja) * almartcajas.piezascaja end ) / ((venliped.bulto_hasta_orden_carga - venliped.bulto_desde_orden_carga) +1)) as alm_bultos_lineas_cantidad,
			formatos.abreviado
    FROM venliped,   
         articulos,   
         almartcajas,   
         formatos,
			ventipolin  
   WHERE ( venliped.empresa  = articulos.empresa ) and  
         ( venliped.articulo = articulos.codigo ) and  
         ( venliped.empresa  = almartcajas.empresa ) and  
         ( venliped.articulo = almartcajas.articulo ) and  
         ( venliped.caja     = almartcajas.caja ) and  
         ( articulos.empresa = formatos.empresa ) and  
         ( articulos.formato = formatos.codigo ) and  
			( venliped.empresa  = ventipolin.empresa ) and 
			( venliped.tipolinea = ventipolin.codigo ) and 
         ( ( venliped.empresa  = :arg_empresa ) and 
			  ( venliped.id_alm_orden_carga = :arg_orden_carga ) and 
			  ( venliped.bulto_desde_orden_carga <= :arg_bulto_orden_carga and venliped.bulto_hasta_orden_carga >= :arg_bulto_orden_carga ) )   
ORDER BY venliped.anyo,venliped.pedido,venliped.linea 
