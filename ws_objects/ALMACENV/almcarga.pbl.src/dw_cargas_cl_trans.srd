 retrieve="select id,fecha, cliente, cliente.razon, isnull(trans.descripcion, '**SIN ASIGNAR**') as descripcion,
case when carga.centro_logistico = '1' then 'TEN'
     when carga.centro_logistico = '2' then 'WOW'
	 else '??' end as CL,
carga.confirmada
from (select * from alm_orden_carga where empresa = :empresa and fecha = :fecha and almtipotransportes_codigo = :codigo_transporte
									and centro_logistico = :centro_logistico) as carga
left join (select * from almtipotransportes where empresa = :empresa) as trans on carga.almtipotransportes_codigo = trans.codigo

left join (select * from genter where empresa = :empresa and tipoter = 'C') as cliente on cliente.codigo = carga.cliente" arguments=(("empresa", string),("fecha", datetime),("codigo_transporte", string),("centro_logistico", string)) )
