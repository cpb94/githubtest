 retrieve="PBSELECT( VERSION(400) TABLE(NAME=~"almlinubica~" )  TABLE(NAME=~"formatos~" )  TABLE(NAME=~"calidades~" )  TABLE(NAME=~"articulos~" ) COLUMN(NAME=~"almlinubica.empresa~") COLUMN(NAME=~"almlinubica.tono~") COLUMN(NAME=~"almlinubica.calibre~") COLUMN(NAME=~"calidades.abreviado~") COLUMN(NAME=~"articulos.descripcion~") COMPUTE(NAME=~"sum(existencias)  total~") COLUMN(NAME=~"almlinubica.calidad~") COLUMN(NAME=~"almlinubica.articulo~") COLUMN(NAME=~"almlinubica.referencia~") COMPUTE(NAME=~"'S'  marca~") COLUMN(NAME=~"formatos.abreviado~") COMPUTE(NAME=~"0.00 reservado~") COMPUTE(NAME=~"0.00 fabricar~") COMPUTE(NAME=~"0.00 preparado~") COLUMN(NAME=~"articulos.fecha_fin~")    JOIN (LEFT=~"almlinubica.empresa~"    OP =~"=~"RIGHT=~"formatos.empresa~"    OUTER1 =~"almlinubica.empresa~" )    JOIN (LEFT=~"almlinubica.formato~"    OP =~"=~"RIGHT=~"formatos.codigo~"    OUTER1 =~"almlinubica.formato~" )    JOIN (LEFT=~"almlinubica.empresa~"    OP =~"=~"RIGHT=~"calidades.empresa~"    OUTER1 =~"almlinubica.empresa~" )    JOIN (LEFT=~"almlinubica.calidad~"    OP =~"=~"RIGHT=~"calidades.codigo~"    OUTER1 =~"almlinubica.calidad~" )    JOIN (LEFT=~"almlinubica.empresa~"    OP =~"=~"RIGHT=~"articulos.empresa~"    OUTER1 =~"almlinubica.empresa~" )    JOIN (LEFT=~"almlinubica.articulo~"    OP =~"=~"RIGHT=~"articulos.codigo~"    OUTER1 =~"almlinubica.articulo~" )WHERE(    EXP1 =~"( almlinubica.empresa~"   OP =~"=~"    EXP2 =~":arg_emp )~"    LOGIC =~"and~" ) WHERE(    EXP1 =~"( almlinubica.calidad~"   OP =~"in~"    EXP2 =~"( :calidades) )~"    LOGIC =~"and~" ) WHERE(    EXP1 =~"almlinubica.sector~"   OP =~"=~"    EXP2 =~"'S'~" )  GROUP(NAME=~"almlinubica.empresa~") GROUP(NAME=~"formatos.abreviado~") GROUP(NAME=~"almlinubica.referencia~") GROUP(NAME=~"almlinubica.articulo~") GROUP(NAME=~"articulos.descripcion~") GROUP(NAME=~"articulos.fecha_fin~") GROUP(NAME=~"almlinubica.calidad~") GROUP(NAME=~"calidades.abreviado~") GROUP(NAME=~"almlinubica.tono~") GROUP(NAME=~"almlinubica.calibre~") GROUP(NAME=~"almlinubica.tipo_pallet~")) ORDER(NAME=~"almlinubica.empresa~" ASC=yes )  ORDER(NAME=~"almlinubica.referencia~" ASC=yes )  ORDER(NAME=~"articulos.descripcion~" ASC=yes )  ORDER(NAME=~"calidades.abreviado~" ASC=yes )  ORDER(NAME=~"almlinubica.tono~" ASC=yes )  ORDER(NAME=~"almlinubica.calibre~" ASC=yes ) ARG(NAME = ~"arg_emp~" TYPE = string)  ARG(NAME = ~"calidades~" TYPE = stringlist) " arguments=(("arg_emp", string),("calidades", stringlist)) )
