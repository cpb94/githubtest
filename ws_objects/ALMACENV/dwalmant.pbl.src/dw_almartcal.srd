 retrieve="  SELECT almartcal.empresa,   
         almartcal.articulo,   
         almartcal.calidad,   
         almartcal.precio,   
         almartcal.fecha_alta,   
         almartcal.stockmin,   
         almartcal.stockmax,   
         almartcal.puntopedido,
		almartcal.fecha_liquidacion,
		almartcal.porcentaje_liquidacion,
		almartcal.precio_liquidacion
    FROM almartcal  
   WHERE ( almartcal.empresa = :codigo_empresa ) AND  
         ( almartcal.articulo = :codigo_articulo ) AND  
         ( almartcal.calidad = :codigo_calidad )   
ORDER BY almartcal.empresa ASC,   
         almartcal.articulo ASC,   
         almartcal.calidad ASC   
" update="almartcal" updatewhere=1 updatekeyinplace=no arguments=(("codigo_empresa", string),("codigo_articulo", string),("codigo_calidad", string)) )
