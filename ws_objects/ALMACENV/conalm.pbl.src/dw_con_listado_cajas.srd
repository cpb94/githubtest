 column=(type=char(20) updatewhereclause=yes name=articulos_codigo_almacen dbname="articulos.codigo_almacen" )
 retrieve="
select almcajas.empresa, almcajas.codigo as codigo_ventas, almcajas.descripcion, almcajas.descripcion_abr, 
almcajas.codigo_compras as codigo_compras, almcajas.certificacion_ccc, articulos.codigo as codigo_almacen
from almcajas
inner join articulos on almcajas.empresa = articulos.empresa and almcajas.codigo_compras = articulos.codigo_compras
where almcajas.codigo_compras = :codigo_caja_compras
and almcajas.empresa = :empresa
" arguments=(("empresa", string),("codigo_caja_compras", string)) )
