global function integer f_max_almlinubica (string empresa, string ubicacion);Integer var_linea 
SELECT max(almlinubica.linea)  
INTO :var_linea  
FROM almlinubica  
WHERE ( almlinubica.empresa = :empresa )
AND   ( almlinubica.ubicacion = :ubicacion ) ;  
Return var_linea

end function

