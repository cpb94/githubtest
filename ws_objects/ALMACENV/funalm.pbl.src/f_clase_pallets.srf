global function string f_clase_pallets (string empresa, string tipo_pallet);String clase

  SELECT pallets.clase  
    INTO :clase  
    FROM pallets  
   WHERE ( pallets.empresa = :empresa ) AND  
         ( pallets.codigo = :tipo_pallet )   ;

Return clase
end function

