         promseries.codigo,   
         promseries.cliente,   
         promseries.tipo_expositor,   
         promseries.situacion,   
         promseries.proveedor,   
         promseries.fpedido,   
         promseries.caracteristicas,   
         promseries.anyo,   
         promseries.pedido  
    FROM promseries  
   WHERE promseries.empresa = :empresa    
