 retrieve="PBSELECT( VERSION(400) TABLE(NAME=~"promped~" )  TABLE(NAME=~"genter~" )  TABLE(NAME=~"vencliproforma~" ) COLUMN(NAME=~"promped.empresa~") COLUMN(NAME=~"promped.anyo~") COLUMN(NAME=~"promped.pedido~") COLUMN(NAME=~"promped.fpedido~") COLUMN(NAME=~"promped.cliente~") COLUMN(NAME=~"promped.expositor~") COLUMN(NAME=~"promped.flistado~") COLUMN(NAME=~"promped.situacion~") COLUMN(NAME=~"promped.tipo_expositor~") COLUMN(NAME=~"promped.tipo~") COLUMN(NAME=~"promped.numserie~") COLUMN(NAME=~"promped.tipo_cliente~") COLUMN(NAME=~"genter.razon~") COLUMN(NAME=~"genter.nombre~") COLUMN(NAME=~"genter.apellidos~") COLUMN(NAME=~"vencliproforma.nombre~") COLUMN(NAME=~"vencliproforma.razon~") COLUMN(NAME=~"promped.clase_panel~")    JOIN (LEFT=~"promped.empresa~"    OP =~"=~"RIGHT=~"genter.empresa~"    OUTER1 =~"promped.empresa~" )    JOIN (LEFT=~"promped.cliente~"    OP =~"=~"RIGHT=~"genter.codigo~"    OUTER1 =~"promped.cliente~" )    JOIN (LEFT=~"promped.empresa~"    OP =~"=~"RIGHT=~"vencliproforma.empresa~"    OUTER1 =~"promped.empresa~" )    JOIN (LEFT=~"promped.cliente~"    OP =~"=~"RIGHT=~"vencliproforma.codigo~"    OUTER1 =~"promped.cliente~" )WHERE(    EXP1 =~"( promped.empresa~"   OP =~"=~"    EXP2 =~":empresa )~"    LOGIC =~"and~" ) WHERE(    EXP1 =~"promped.situacion~"   OP =~"<>~"    EXP2 =~"'S'~"    LOGIC =~"and~" ) WHERE(    EXP1 =~"promped.tipo~"   OP =~"=~"    EXP2 =~":tipos~"    LOGIC =~"and~" ) WHERE(    EXP1 =~"genter.tipoter~"   OP =~"=~"    EXP2 =~"'C'~" ) ) ORDER(NAME=~"promped.empresa~" ASC=yes )  ORDER(NAME=~"promped.anyo~" ASC=yes )  ORDER(NAME=~"promped.cliente~" ASC=yes )  ORDER(NAME=~"promped.fpedido~" ASC=yes ) ARG(NAME = ~"empresa~" TYPE = string)  ARG(NAME = ~"tipos~" TYPE = string) " arguments=(("empresa", string),("tipos", string)) )
