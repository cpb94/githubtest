global function string f_grupo_prompaneles (string ra_codemp, string ra_codigo); string ls_nombre

 SELECT prompaneles.grupo
    INTO :ls_nombre
    FROM prompaneles 
   WHERE ( prompaneles.empresa = :ra_codemp ) AND  
         ( prompaneles.codigo = :ra_codigo )  
  USING SQLCA;

Return ls_nombre

end function

