global function decimal f_pesopanel_promparam (string arg_empresa);Dec{4}  argumento
  SELECT promparam.pesopanelmano  
    INTO :argumento  
    FROM promparam  
   WHERE promparam.empresa = :arg_empresa   ;
	return argumento

end function

