global function decimal f_pventapanel_promparam (string arg_empresa);Dec  argumento

SELECT promparam.pventapanelmano  
INTO   :argumento  
FROM   promparam  
WHERE  promparam.empresa = :arg_empresa;

if isnull(argumento) then argumento = 0

return argumento

end function

