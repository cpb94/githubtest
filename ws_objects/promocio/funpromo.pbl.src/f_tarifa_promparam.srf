global function string f_tarifa_promparam (string arg_empresa);String  var_tarifa

  SELECT promparam.tarifa INTO :var_tarifa
    FROM promparam  
   WHERE promparam.empresa = :arg_empresa   ;
	
Return var_tarifa;

end function

