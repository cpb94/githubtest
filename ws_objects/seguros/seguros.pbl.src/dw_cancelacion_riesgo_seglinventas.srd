 retrieve="PBSELECT( VERSION(400) TABLE(NAME=~"seglinventas~" )  TABLE(NAME=~"venriesgoclientes~" )  TABLE(NAME=~"genter~" ) COLUMN(NAME=~"seglinventas.empresa~") COLUMN(NAME=~"seglinventas.anyo~") COLUMN(NAME=~"seglinventas.recibo~") COLUMN(NAME=~"seglinventas.ffra~") COLUMN(NAME=~"seglinventas.cliente~") COLUMN(NAME=~"seglinventas.fcobro~") COLUMN(NAME=~"seglinventas.fvto~") COLUMN(NAME=~"seglinventas.importe~") COLUMN(NAME=~"seglinventas.moneda~") COLUMN(NAME=~"seglinventas.cambiofra~") COLUMN(NAME=~"seglinventas.declarado~") COLUMN(NAME=~"seglinventas.f_notificacion~") COLUMN(NAME=~"seglinventas.cuenta~") COLUMN(NAME=~"seglinventas.companyia~") COLUMN(NAME=~"seglinventas.forma_pago~") COLUMN(NAME=~"seglinventas.situacion~") COLUMN(NAME=~"seglinventas.fdev~") COLUMN(NAME=~"seglinventas.impdev~") COLUMN(NAME=~"venriesgoclientes.numero~") COLUMN(NAME=~"genter.razon~") COLUMN(NAME=~"seglinventas.orden~") COLUMN(NAME=~"seglinventas.fra~")    JOIN (LEFT=~"seglinventas.empresa~"    OP =~"=~"RIGHT=~"venriesgoclientes.empresa~" )    JOIN (LEFT=~"seglinventas.cuenta~"    OP =~"=~"RIGHT=~"venriesgoclientes.cuenta~" )    JOIN (LEFT=~"seglinventas.empresa~"    OP =~"=~"RIGHT=~"genter.empresa~" )    JOIN (LEFT=~"seglinventas.cliente~"    OP =~"=~"RIGHT=~"genter.codigo~" )WHERE(    EXP1 =~"( ( seglinventas.empresa~"   OP =~"=~"    EXP2 =~":empresa )~"    LOGIC =~"and~" ) WHERE(    EXP1 =~"( genter.tipoter~"   OP =~"=~"    EXP2 =~"'C' ) )~"    LOGIC =~"and~" ) WHERE(    EXP1 =~"( seglinventas.situacion~"   OP =~"in~"    EXP2 =~"(~~~"1~~~",~~~"2~~~",~~~"3~~~") )~"    LOGIC =~"and~" ) WHERE(    EXP1 =~"( seglinventas.fvto~"   OP =~"<=~"    EXP2 =~":fecha )~"    LOGIC =~"and~" ) WHERE(    EXP1 =~"( seglinventas.forma_pago~"   OP =~"not in~"    EXP2 =~"(  SELECT segfpagoexcluidas.forma_pago      FROM segfpagoexcluidas      WHERE ( segfpagoexcluidas.empresa = :empresa )  ))~" ) ) ORDER(NAME=~"seglinventas.fvto~" ASC=no) ARG(NAME = ~"empresa~" TYPE = string)  ARG(NAME = ~"fecha~" TYPE = datetime) " arguments=(("empresa", string),("fecha", datetime)) )
