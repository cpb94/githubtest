 retrieve="  SELECT  seglinventas.empresa ,           seglinventas.anyo ,           seglinventas.fra ,           seglinventas.recibo ,           seglinventas.ffra ,           seglinventas.cliente ,           seglinventas.fcobro ,           seglinventas.fvto ,           seglinventas.importe ,           seglinventas.moneda ,           seglinventas.cambiofra ,           seglinventas.declarado ,           seglinventas.orden ,           seglinventas.f_notificacion ,           seglinventas.cuenta ,           seglinventas.companyia ,           seglinventas.forma_pago ,           seglinventas.situacion ,           seglinventas.fdev ,           seglinventas.impdev 
        FROM seglinventas      
        WHERE ( seglinventas.empresa = :empresa ) and          ( seglinventas.ffra >= :fecha1 ) and          ( seglinventas.ffra <= :fecha2 )  
        ORDER BY seglinventas.cliente          ASC,           seglinventas.ffra          ASC,           seglinventas.fra          ASC,           seglinventas.recibo          ASC  " update="seglinventas" updatewhere=1 updatekeyinplace=no arguments=(("empresa", string),("fecha1", datetime),("fecha2", datetime)) )
