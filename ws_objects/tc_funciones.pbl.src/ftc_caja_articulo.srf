global function string ftc_caja_articulo (string empresa, string articulo);String caja = ""

SELECT TOP 1 caja INTO :caja
FROM almartcajas 
WHERE empresa = :empresa 
AND articulo = :articulo 
AND pordefecto = 'S';

return caja
end function

