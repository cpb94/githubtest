 retrieve="  SELECT carfinan.bancofinan,   
         carfinan.contador,   
         carfinan.num_prorroga,   
         carfinan.vencimiento,   
         carfinan.cliente,   
         carfinan.moneda,   
         carfinan.divisasfinan,   
         carfinan.factura,   
         carfinan.anyo_fac,   
         carfinan.fpago,   
         carfinan.fanticipo,   
         carfinan.fvalor,   
         carfinan.importe_abono,   
         carfinan.empresa,   
         venfac.ffactura  
    FROM carfinan,   
         venfac  
   WHERE ( carfinan.empresa = venfac.empresa ) and  
         ( carfinan.anyo_fac = venfac.anyo ) and  
         ( convert(numeric(16,0),carfinan.factura) = venfac.factura ) and  
         ( ( carfinan.empresa = :arg_empresa ) AND  
         ( carfinan.bancofinan = :arg_bancofinan ) AND  
         ( carfinan.anyo = :arg_anyo ) AND  
         ( carfinan.contador = :arg_contador ) AND  
         ( carfinan.num_prorroga = :arg_num_prorroga ) )    
" arguments=(("arg_empresa", string),("arg_anyo", number),("arg_bancofinan", string),("arg_num_prorroga", number),("arg_contador", number),("arg_cabecera", string),("arg_importe", number),("arg_moneda_emp", string),("arg_documentos", string)) )
