 retrieve="SELECT  carefectos.empresa ,
           carefectos.anyo ,
           carefectos.factura ,
           carefectos.ffra ,
           carefectos.importe ,
           carefectos.divisas ,
           carefectos.monedas ,
           carefectos.banco ,
           carefectos.Nacional ,
           carefectos.cliente ,
           carefectos.serie ,
           ~"N~" punt1,
           ~"N~" punt2,
           carefectos.orden ,
           carefectos.anyofra ,
           carefectos.recibo ,
           carefectos.fvto ,
           carefectos.cambio ,
           carefectos.fpago     
        FROM carefectos      
        WHERE ( carefectos.empresa = :arg_empresa ) and
          ( carefectos.anyofra between :arg_anyo_desde and :arg_anyo_hasta ) and
          ( carefectos.cliente = :arg_cliente ) and
          ( carefectos.Nacional = :arg_nacional )   and
          ( carefectos.situacion in (select isnull(cartipodoc.situacion_pase_cartera,'') from cartipodoc where cartipodoc.empresa = :arg_empresa group by isnull(cartipodoc.situacion_pase_cartera,'')))" update="carefectos" updatewhere=0 updatekeyinplace=no arguments=(("arg_empresa", string),("arg_anyo_desde", number),("arg_anyo_hasta", number),("arg_cliente", string),("arg_nacional", string))  sort="factura A " )
