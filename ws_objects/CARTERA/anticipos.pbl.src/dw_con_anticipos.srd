table(column=(type=char(5) updatewhereclause=yes name=empresa dbname="carfinan.empresa" )
 column=(type=char(5) updatewhereclause=yes name=bancofinan dbname="carfinan.bancofinan" )
 column=(type=long updatewhereclause=yes identity=yes name=contador dbname="carfinan.contador" )
 column=(type=long updatewhereclause=yes identity=yes name=num_prorroga dbname="carfinan.num_prorroga" )
 column=(type=decimal(5) updatewhereclause=yes identity=yes name=divisasfinan dbname="carfinan.divisasfinan" )
 column=(type=long updatewhereclause=yes identity=yes name=situacion dbname="carfinan.situacion" values="No Impreso	0/Impreso	1/Abonado	2/Abonado	3/Cancelado	4/Anulado	5/" )
 column=(type=datetime updatewhereclause=yes name=fvalor dbname="carfinan.fvalor" )
 column=(type=char(5) updatewhereclause=yes name=moneda dbname="carfinan.moneda" )
 column=(type=long updatewhereclause=yes identity=yes name=prorroga_activa dbname="carfinan.prorroga_activa" )
 column=(type=char(20) updatewhereclause=yes name=cliente dbname="carfinan.cliente" )
 column=(type=decimal(5) updatewhereclause=yes identity=yes name=importe_abono dbname="carfinan.importe_abono" )
 column=(type=char(20) updatewhereclause=yes name=factura dbname="carfinan.factura" )
 column=(type=char(5) updatewhereclause=yes name=serie_fac dbname="carfinan.serie_fac" )
 column=(type=datetime updatewhereclause=yes name=vencimiento dbname="carfinan.vencimiento" )
 column=(type=char(30) updatewhereclause=yes name=refbanco dbname="carfinan.refbanco" )
 column=(type=long updatewhereclause=yes identity=yes name=anyo_fac dbname="carfinan.anyo_fac" )
 column=(type=datetime updatewhereclause=yes name=fecha_cobro dbname="carfinan.fecha_cobro" )
 retrieve="  SELECT carfinan.empresa,   
         carfinan.bancofinan,   
         carfinan.contador,   
         carfinan.num_prorroga,   
         carfinan.divisasfinan,   
         carfinan.situacion,   
         carfinan.fvalor,   
         carfinan.moneda,   
         carfinan.prorroga_activa,   
         carfinan.cliente,   
         carfinan.importe_abono,   
         carfinan.factura,   
         carfinan.serie_fac,   
         carfinan.vencimiento,   
         carfinan.refbanco,   
         carfinan.anyo_fac,   
         fecha_abono as fecha_cobro  
    FROM carfinan  
   WHERE ( carfinan.empresa = :arg_empresa ) AND  
         ( carfinan.bancofinan like :arg_banco ) AND  
         ( carfinan.situacion between :arg_situd and :arg_situh ) AND  
         ( carfinan.anyo = :arg_anyo )    
UNION ALL
  SELECT carfinan.empresa,   
         carfinan.bancofinan,   
         carfinan.contador,   
         carfinan.num_prorroga,   
         carfinan.divisasfinan,   
         '4' as situacion,   
         carfinan.fvalor,   
         carfinan.moneda,   
         carfinan.prorroga_activa,   
         carfinan.cliente,   
         (carfinan.divisasfinan - carfinan.importe_abono) as importe_abono,   
         carfinan.factura,   
         carfinan.serie_fac,   
         carfinan.vencimiento,   
         carfinan.refbanco,   
         carfinan.anyo_fac,   
         fecha_abono as fecha_cobro  
    FROM carfinan  
   WHERE ( carfinan.empresa = :arg_empresa ) AND  
         ( carfinan.bancofinan like :arg_banco ) AND  
			( carfinan.situacion in ('2','3') and carfinan.divisasfinan <> carfinan.importe_abono ) and 
         ( '4' between :arg_situd and :arg_situh ) AND  
         ( carfinan.anyo = :arg_anyo )    
" arguments=(("arg_empresa", string),("arg_anyo", number),("arg_banco", string),("arg_situd", number),("arg_situh", number))  sort="empresa A bancofinan A contador A num_prorroga A situacion A " )
