global function string f_codpostal_banco (string cod_empresa, string cod_banco);String ls_direccion

 SELECT    carbancos.codpostal
    INTO   :ls_direccion
    FROM carbancos 
   WHERE ( carbancos.empresa= :cod_empresa ) AND  
         ( carbancos.banco  = :cod_banco   )   ;

RETURN ls_direccion
//

end function

