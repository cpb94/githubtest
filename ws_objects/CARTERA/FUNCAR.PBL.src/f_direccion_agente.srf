global function string f_direccion_agente (string cod_empresa, string cod_agente);String ls_direc

 SELECT    venagentes.direccion1
    INTO   :ls_direc
    FROM venagentes 
   WHERE ( venagentes.empresa= :cod_empresa ) AND  
         ( venagentes.agente= :cod_agente   )   ;

RETURN ls_direc
//

end function

