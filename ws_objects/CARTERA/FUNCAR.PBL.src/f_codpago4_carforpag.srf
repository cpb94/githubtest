global function string f_codpago4_carforpag (string arg_empresa, string arg_codigo);string retorno

select codpago4
into :retorno
from carforpag
where empresa=:arg_empresa and
		forma=:arg_codigo;
		
if isnull(retorno) or retorno='' then retorno=arg_codigo

return retorno
end function

