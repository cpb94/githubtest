global function string f_remesable_carforpag (string empresa, string codigo);String var_remesable

SELECT carforpag.remesable
INTO :var_remesable
FROM carforpag  
WHERE carforpag.empresa = :empresa
AND   carforpag.forma = :codigo;

Return var_remesable

end function

