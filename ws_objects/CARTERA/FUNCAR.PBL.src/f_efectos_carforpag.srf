global function decimal f_efectos_carforpag (string empresa, string codigo);Dec var
SELECT carforpag.efectos    INTO :var
    FROM carforpag  
   WHERE ( carforpag.empresa = :empresa ) AND  
         ( carforpag.forma = :codigo )   ;

Return var

end function

