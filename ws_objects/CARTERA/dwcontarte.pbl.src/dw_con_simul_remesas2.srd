table(column=(type=decimal(6) updatewhereclause=yes name=remesa dbname="carefectos.remesa" dbalias=".remesa" )
 column=(type=datetime updatewhereclause=yes name=fremesa dbname="carefectos.fremesa" dbalias=".fremesa" )
 column=(type=decimal(6) updatewhereclause=yes identity=yes name=importe dbname="importe" )
 retrieve="  SELECT carefectos.remesa,   
         carefectos.fremesa,   
         Sum(importe)  as importe
    FROM carefectos  
   WHERE ( carefectos.empresa = :empresa ) AND  
         ( carefectos.banco = :banco ) AND  
         ( carefectos.situacion = '1' ) AND  
		( carefectos.anyo_remesa = :arg_anyo ) and 
         ( carefectos.Nacional = :arg_nacional )   
GROUP BY carefectos.remesa,   
         carefectos.fremesa  
ORDER BY carefectos.remesa ASC,   
         carefectos.fremesa ASC   
" arguments=(("empresa", string),("banco", string),("arg_anyo", number),("arg_nacional", string)) )
