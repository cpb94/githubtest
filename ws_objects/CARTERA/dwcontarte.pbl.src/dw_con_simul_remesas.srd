 retrieve="PBSELECT( VERSION(400) TABLE(NAME=~"carefectos~" )  TABLE(NAME=~"genter~" )  TABLE(NAME=~"monedas~" ) COLUMN(NAME=~"carefectos.empresa~") COLUMN(NAME=~"carefectos.anyo~") COLUMN(NAME=~"carefectos.orden~") COLUMN(NAME=~"carefectos.anyofra~") COLUMN(NAME=~"carefectos.recibo~") COLUMN(NAME=~"carefectos.fvto~") COLUMN(NAME=~"carefectos.ffra~") COLUMN(NAME=~"carefectos.importe~") COLUMN(NAME=~"carefectos.divisas~") COLUMN(NAME=~"carefectos.monedas~") COLUMN(NAME=~"carefectos.libre~") COLUMN(NAME=~"carefectos.factura~") COLUMN(NAME=~"carefectos.remesa~") COLUMN(NAME=~"genter.razon~") COLUMN(NAME=~"carefectos.Nacional~") COLUMN(NAME=~"carefectos.tipodoc~") COLUMN(NAME=~"monedas.abreviado~") COLUMN(NAME=~"carefectos.fremesa~") COLUMN(NAME=~"carefectos.cliente~")    JOIN (LEFT=~"carefectos.empresa~"    OP =~"=~"RIGHT=~"genter.empresa~"    OUTER1 =~"carefectos.empresa~" )    JOIN (LEFT=~"carefectos.cliente~"    OP =~"=~"RIGHT=~"genter.codigo~"    OUTER1 =~"carefectos.cliente~" )    JOIN (LEFT=~"carefectos.monedas~"    OP =~"=~"RIGHT=~"monedas.moneda~"    OUTER1 =~"carefectos.monedas~" )WHERE(    EXP1 =~" carefectos.empresa~"   OP =~"=~"    EXP2 =~":empresa ~"    LOGIC =~"and~" ) WHERE(    EXP1 =~"carefectos.banco~"   OP =~"in~"    EXP2 =~"(:banco,null)~"    LOGIC =~"and~" ) WHERE(    EXP1 =~"carefectos.anyo_remesa~"   OP =~"in~"    EXP2 =~"(:anyo_remesa,null)~"    LOGIC =~"and~" ) WHERE(    EXP1 =~"carefectos.remesa~"   OP =~"in~"    EXP2 =~"(:remesa,null)~"    LOGIC =~"and~" ) WHERE(    EXP1 =~"carefectos.situacion~"   OP =~"=~"    EXP2 =~"'0'~"    LOGIC =~"and~" ) WHERE(    EXP1 =~"genter.tipoter~"   OP =~"=~"    EXP2 =~"'C'~"    LOGIC =~"and~" ) WHERE(    EXP1 =~"carefectos.Nacional~"   OP =~"=~"    EXP2 =~"'S'~"    LOGIC =~"and~" ) WHERE(    EXP1 =~"carefectos.fvto~"   OP =~"<=~"    EXP2 =~":fecha1~" ) ) ORDER(NAME=~"carefectos.empresa~" ASC=yes )  ORDER(NAME=~"carefectos.remesa~" ASC=no)  ORDER(NAME=~"carefectos.fvto~" ASC=yes )  ORDER(NAME=~"carefectos.factura~" ASC=yes ) ARG(NAME = ~"empresa~" TYPE = string)  ARG(NAME = ~"anyo_remesa~" TYPE = number)  ARG(NAME = ~"banco~" TYPE = string)  ARG(NAME = ~"remesa~" TYPE = number)  ARG(NAME = ~"fecha1~" TYPE = datetime) " arguments=(("empresa", string),("anyo_remesa", number),("banco", string),("remesa", number),("fecha1", datetime)) )
