 retrieve="  SELECT prodmeses.fecha,   
         prodmeses.horas,   
         prodmeses.calendario,   
         prodmeses.dia,   
         prodmeses.mes,   
         prodmeses.anyo,   
         prodmeses.empresa ,
		datepart (weekday, fecha) dia_semana,
		datename (weekday, fecha) dia_semana_texto,
		datename (day, fecha) dia_numero,
		datename (month, fecha) mes_texto,
		datename (week, fecha) semana_numero
 
    FROM prodmeses  
   WHERE ( prodmeses.empresa = :empresa ) AND  
         ( prodmeses.mes =  :mes)    and
		( prodmeses.anyo = :anyo) and 
		prodmeses.calendario = :calendario
order by datename (week, fecha), datepart (weekday, fecha)
" arguments=(("empresa", string),("calendario", string),("mes", number),("anyo", number))  sort="semana_numero A " )
