 column=(type=char(140) updatewhereclause=yes name=articulos_descripcion dbname="articulos.descripcion" )
 column=(type=char(10) updatewhereclause=yes name=articulos_molde dbname="articulos.molde" )
 column=(type=char(10) updatewhereclause=yes name=articulos_plato_inf dbname="articulos.plato_inf" )
 column=(type=char(30) updatewhereclause=yes name=formatos_descripcion dbname="formatos.descripcion" )
 column=(type=decimal(6) updatewhereclause=yes name=formatos_ancho dbname="formatos.ancho" )
 column=(type=decimal(6) updatewhereclause=yes name=formatos_largo dbname="formatos.largo" )
 column=(type=char(5) updatewhereclause=yes name=articulos_familia dbname="articulos.familia" )
 column=(type=decimal(0) updatewhereclause=yes name=articulos_lote_minimo dbname="articulos.lote_minimo" )
 column=(type=decimal(0) updatewhereclause=yes name=articulos_stock_min dbname="articulos.stock_min" )
 column=(type=decimal(0) updatewhereclause=yes name=articulos_stocksugerido dbname="articulos.stocksugerido" )
 column=(type=char(1) updatewhereclause=yes name=articulos_promocion dbname="articulos.promocion" )
 column=(type=decimal(11) updatewhereclause=yes identity=yes name=venliped_cantidad dbname="cantidad" )
 column=(type=char(1) updatewhereclause=yes name=marcado dbname="articulos.marcado" values="	S/	N" )
 column=(type=long updatewhereclause=yes identity=yes name=sugerido2 dbname="sugerido2" )
 column=(type=long updatewhereclause=yes identity=yes name=stock_biz dbname="stock_biz" )
 column=(type=decimal(6) updatewhereclause=yes identity=yes name=stock dbname="stock" )
 column=(type=decimal(0) updatewhereclause=yes identity=yes name=planificado dbname="planificado" )
 column=(type=decimal(11) updatewhereclause=yes identity=yes name=consumo dbname="consumo" )
 column=(type=decimal(11) updatewhereclause=yes identity=yes name=consumo_min dbname="consumo_min" )
 column=(type=decimal(11) updatewhereclause=yes identity=yes name=consumo_60d dbname="consumo_60D" )
 retrieve="SELECT articulos.codigo,   
         articulos.formato,   
         articulos.descripcion,   
         articulos.molde,   
         articulos.plato_inf,   
         formatos.descripcion,   
		formatos.ancho,
		formatos.largo,
         articulos.familia  ,
		articulos.lote_minimo,
		articulos.stock_min,
		articulos.stocksugerido,
		articulos.promocion,
		( select isnull (sum (case when venliped.tipo_unidad  = 0 then venliped.cantidad else 
					 case when almartcajas.piezascaja < 999 then ( ( venliped.cantidad / almartcajas.metroscaja ) * almartcajas.piezascaja ) else  venliped.cantidad / ((formatos.ancho * formatos.largo) /10000 ) END END), 0 ) 
		from venliped 
		inner join formatos on venliped.empresa = formatos.empresa and venliped.formato = formatos.codigo
		inner join venped on venliped.empresa = venped.empresa and venliped.anyo = venped.anyo and venliped.pedido = venped.pedido
		inner join almartcajas on venliped.empresa = almartcajas.empresa and venliped.articulo = almartcajas.articulo  and venliped.caja = almartcajas.caja 
		where venliped.articulo =  articulos.codigo and  venped.es_proforma <> 'S'  ) cantidad,		
		~" ~" as marcado,
		0 as sugerido2,
		0 as stock_biz,
		( select sum (almacenmovimientos.cantidade - almacenmovimientos.cantidads)
		from almacenmovimientos where almacenmovimientos.articulo = articulos.codigo and almacenmovimientos.calidad = '1') stock,
		
		( select sum ( cantidad ) from planificacion where cod_articulo = articulos.codigo ) planificado,
		
		( select isnull (sum (case when venlifac.tipo_unidad  = 0 then venlifac.cantidad else 
					 case when almartcajas.piezascaja < 999 then ( ( venlifac.cantidad / almartcajas.metroscaja ) * almartcajas.piezascaja ) else  venlifac.cantidad / ((formatos.ancho * formatos.largo) /10000 ) END END), 0 ) 
		from venlifac
		inner join formatos on venlifac.empresa = formatos.empresa and venlifac.formato = formatos.codigo
		inner join almartcajas on venlifac.empresa = almartcajas.empresa and venlifac.articulo = almartcajas.articulo  and venlifac.caja = almartcajas.caja 
		where venlifac.articulo =  articulos.codigo and venlifac.ffactura >= :fecha1 ) consumo,
		
		( select isnull (sum (case when venlifac.tipo_unidad  = 0 then venlifac.cantidad else 
					 case when almartcajas.piezascaja < 999 then ( ( venlifac.cantidad / almartcajas.metroscaja ) * almartcajas.piezascaja ) else  venlifac.cantidad / ((formatos.ancho * formatos.largo) /10000 ) END END), 0 ) 
		from venlifac
		inner join formatos on venlifac.empresa = formatos.empresa and venlifac.formato = formatos.codigo
		inner join almartcajas on venlifac.empresa = almartcajas.empresa and venlifac.articulo = almartcajas.articulo  and venlifac.caja = almartcajas.caja 
		where venlifac.articulo =  articulos.codigo and venlifac.ffactura >= :fecha2 ) consumo_min,

		( select isnull (sum (case when venlifac.tipo_unidad  = 0 then venlifac.cantidad else 
					 case when almartcajas.piezascaja < 999 then ( ( venlifac.cantidad / almartcajas.metroscaja ) * almartcajas.piezascaja ) else  venlifac.cantidad / ((formatos.ancho * formatos.largo) /10000 ) END END), 0 ) 
		from venlifac
		inner join formatos on venlifac.empresa = formatos.empresa and venlifac.formato = formatos.codigo
		inner join almartcajas on venlifac.empresa = almartcajas.empresa and venlifac.articulo = almartcajas.articulo  and venlifac.caja = almartcajas.caja 
		where venlifac.articulo =  articulos.codigo and venlifac.ffactura >= :fecha3 ) consumo_60D
    FROM articulos,   
         formatos  
   WHERE ( articulos.empresa = formatos.empresa ) and  
         ( articulos.formato = formatos.codigo ) and  
         ( ( articulos.empresa = :empresa ) AND  
         ( articulos.uso = '3' ) and
		 ( articulos.submarcas_codigo in ( :submarca ) ) )


group by    articulos.codigo,
         articulos.formato,   
         articulos.descripcion, 
			articulos.molde,  
			articulos.plato_inf,  
         formatos.descripcion, 
		formatos.ancho,
		formatos.largo,
         articulos.familia,
		articulos.lote_minimo,
		articulos.stock_min,
		articulos.stocksugerido,
		articulos.promocion
order by articulos.descripcion, formatos.descripcion

/*( articulos.familia in ( :familia ) ) and 
		 ( articulos.marcas_codigo in ( :marca ) ) and */
