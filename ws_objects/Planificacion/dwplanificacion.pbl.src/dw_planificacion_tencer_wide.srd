 retrieve="  SELECT (CASE WHEN isnull(planificacion.es_hueco,0) = 1 THEN venliped.descripcion
			ELSE articulos.descripcion
 END) as articulos_descripcion,   
			articulos.control_stock,
			articulos.codigo_pantallas,
		articulos.uso,
         formatos.abreviado,   
         formatos.descripcion,   
         art_ver_tipooperacion.descripcion_abr,   
         planificacion.empresa,   
         planificacion.pedido,   
         planificacion.linped,   
         planificacion.anyo,   
         planificacion.orden,   
         planificacion.lineaprod,   
         planificacion.ot,   
         planificacion.ordenoperacion,   
         planificacion.tipo_operacion,   
         planificacion.stockacabado,   
         planificacion.stockbase,   
         planificacion.stock_cajas,   
         planificacion.cantidad,   
         planificacion.formato,   
         planificacion.cod_articulo,   
         planificacion.version,   
         planificacion.base,   
         planificacion.formato_base,   
         planificacion.f_entrega,  
         venliped.fentrega_inicial,  
         venliped.fecha_entrega2,  
         venliped.fecha_entrega3,  
         venliped.fecha_entrega4,  
         venliped.fecha_entrega5,  
         planificacion.usuario,   
         planificacion.ordenmarcado,   
         planificacion.velocidad,   
         planificacion.fecha_fin,   
         planificacion.hay_obs_ubicaciones,   
         planificacion.fecha_exacta,   
         articulos.molde,   
         art_codestadistico.TM,   
         planificacion.fecha_anterior,   
         planificacion.fecha_sugerida,   
         venliped.fpedido,   
         art_ver_operaciones.numaplicaciones,   
         venliped.fentrega,   
         formatos.descripcion,   
			formatos.ancho,
			formatos.largo,
         venliped.cantidad,   
         genter.razon,   
         venliped.deposito,   
         planificacion.fecha_salida,   
         planificacion.ajuste,   
         articulos.bloqueado,
		articulos.molde,
		articulos.marco,
		articulos.plato_sup,
		articulos.plato_inf,
		isnull(planificacion.es_hueco,0) as planificacion_es_hueco,
			planificacion.art_encajado,
			art_tipomoldura.descripcion,
venliped.planificado,
	planificacion.f_fin_estimada, 
	ISNULL(CONVERT(char(20), (SELECT MIN (venped.fecha_carga_prevista) FROM venped INNER JOIN venliped ON venped.empresa = venliped.empresa AND venped.anyo = venliped.anyo AND venped.pedido = venliped.pedido WHERE venliped.empresa = planificacion.empresa AND venliped.articulo = planificacion.cod_articulo ), 3), '')  AS fecha_carga_prevista_min,
	ISNULL((SELECT case when articulos.unidad = '1' then ROUND(sum(venliped.cantidad) / ((formatos.ancho * formatos.largo) / 10000),0) else sum(venliped.cantidad) end FROM venliped INNER JOIN venped ON venliped.empresa = venped.empresa AND venliped.anyo = venped.anyo AND venliped.pedido = venped.pedido INNER JOIN articulos ON venliped.empresa = articulos.empresa AND venliped.articulo = articulos.codigo INNER JOIN formatos ON articulos.empresa = formatos.empresa AND articulos.formato = formatos.codigo WHERE venliped.empresa = planificacion.empresa AND venliped.articulo = planificacion.cod_articulo and venliped.calidad = '1' and venped.es_proforma = 'N' group by articulos.unidad,formatos.ancho,formatos.largo), 0)  AS pedidos_totales,
	ISNULL((SELECT sum( almacenmovimientos.cantidade - almacenmovimientos.cantidads ) FROM almacenmovimientos WHERE almacenmovimientos.empresa = planificacion.empresa AND almacenmovimientos.articulo = planificacion.cod_articulo AND almacenmovimientos.calidad = '1'),0) AS STOCK,
	isnull((select sum(planificacion_con_fecha.cantidad) from planificacion as planificacion_con_fecha where planificacion_con_fecha.empresa = planificacion.empresa and planificacion_con_fecha.cod_articulo = planificacion.cod_articulo and planificacion_con_fecha.f_fin_estimada is not null ),0) as planificado_con_fecha,
	articulos.familia,
	planificacion.lote,
	planificacion.calibre,
	planificacion.cod_caja,
	planificacion.cod_pallet,
	almartcajas.caja,
	almartcajas.piezascaja,
	vista_cajas.codigo_ventas,
	costilla.descripcion,articulos.contra_stock,
	( SELECT max (comlinpedpro.fecha_entrega) FROM comlinpedpro WHERE comlinpedpro.empresa = vista_cajas.empresa AND  comlinpedpro.mprima = vista_cajas.codigo_compras ) as fecha_entrega_cajas
	
    FROM planificacion 	LEFT OUTER JOIN art_ver_tipooperacion ON 
									planificacion.empresa = art_ver_tipooperacion.empresa AND 
									planificacion.tipo_operacion = art_ver_tipooperacion.codigo 
								LEFT OUTER JOIN venliped ON 
									planificacion.empresa = venliped.empresa AND 
									planificacion.pedido = venliped.pedido AND 
									planificacion.linped = venliped.linea AND 
									planificacion.anyo = venliped.anyo 
								LEFT OUTER JOIN almartcajas ON 
									planificacion.empresa = almartcajas.empresa AND 
									planificacion.art_encajado = almartcajas.articulo AND 
									almartcajas.pordefecto = 'S'
								LEFT OUTER JOIN vista_cajas ON vista_cajas.empresa = almartcajas.empresa AND 
									vista_cajas.codigo_almacen = almartcajas.caja
								LEFT OUTER JOIN venped ON 
									planificacion.empresa = venped.empresa AND 
									planificacion.pedido = venped.pedido AND 
									planificacion.anyo = venped.anyo 
								LEFT OUTER JOIN art_ver_operaciones ON   
							 		planificacion.empresa = art_ver_operaciones.empresa  and  
	         						planificacion.cod_articulo = art_ver_operaciones.articulo  and  
				          			planificacion.version = art_ver_operaciones.version  and  
				          			planificacion.ordenoperacion = art_ver_operaciones.orden 
						 		LEFT OUTER JOIN formatos ON
									art_ver_operaciones.empresa = formatos.empresa and  
         							art_ver_operaciones.formato = formatos.codigo , 
									articulos 
								LEFT OUTER JOIN art_tipomoldura ON articulos.empresa = art_tipomoldura.empresa AND 
									articulos.relieve = art_tipomoldura.codigo
								LEFT OUTER JOIN genter ON 
									articulos.empresa = genter.empresa AND 
									articulos.cliente = genter.codigo
								LEFT OUTER JOIN costilla ON 
									articulos.empresa = costilla.empresa AND 
									articulos.costilla = costilla.codigo,

         art_codestadistico  
   WHERE ( articulos.empresa = art_codestadistico.empresa ) and  
         ( articulos.codigo = art_codestadistico.codigo ) and  
         ( planificacion.empresa = articulos.empresa ) and  
         ( planificacion.cod_articulo = articulos.codigo ) and  
         ( ( planificacion.empresa = :empresa ) AND  
         ( planificacion.lineaprod = :lineaprod ) AND  
         ( genter.tipoter = ~"C~" or genter.tipoter is null) )   
ORDER BY planificacion.orden ASC   
" update="planificacion" updatewhere=0 updatekeyinplace=yes arguments=(("empresa", string),("lineaprod", string))  sort="orden A " )
