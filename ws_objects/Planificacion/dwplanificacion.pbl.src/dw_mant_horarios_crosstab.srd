table(column=(type=number updatewhereclause=yes name=hora dbname="hora" )
 column=(type=char(20) updatewhereclause=yes name=activa dbname="activa" )
 retrieve="  SELECT horarios.fecha,   
         horarios.calendario,   
         horarios.anyo,   
         horarios.mes,   
         CAST(horarios.dia AS INT) AS dias_ordenados,   
         horarios.hora,   
         horarios.activa,
			DATENAME(weekday,fecha) AS nombre_dia,
			DATENAME(month,fecha) AS nombre_mes  
   FROM horarios   
	WHERE horarios.calendario = :calendario
	AND 	horarios.anyo = :anyo
	AND   horarios.empresa = :cod_empresa
	AND	DATENAME(month,horarios.fecha) = :mes
	ORDER BY horarios.fecha
" arguments=(("calendario", string),("anyo", number),("cod_empresa", string),("mes", string))  sort="hora A " )
