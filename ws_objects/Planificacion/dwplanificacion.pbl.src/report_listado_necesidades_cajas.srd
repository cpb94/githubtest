 retrieve="  SELECT planificacion.cod_articulo,   
         planificacion.empresa,   
         articulos.descripcion,   
         almcajas.codigo,   
         almcajas.descripcion,   
         almartcajas.piezascaja,   
         planificacion.cantidad,   
         planificacion.pedido,   
         planificacion.linped,   
         planificacion.anyo,   
         almcajas.codigo_compras,
		B.codigo,
		B.descripcion
    FROM planificacion,   
         articulos,   
         almcajas,   
         almartcajas,
		articulos B
   WHERE ( planificacion.empresa = articulos.empresa ) and  
         ( planificacion.cod_articulo = articulos.codigo ) and  
         ( articulos.empresa = almartcajas.empresa ) and  
         ( articulos.codigo = almartcajas.articulo ) and  
         ( almartcajas.empresa = almcajas.empresa ) and  
         ( almartcajas.caja = almcajas.codigo ) and  
         ( ( planificacion.empresa = :empresa ) )   and
		almartcajas.pordefecto = 'S' and
		B.codigo_compras = almcajas.codigo_compras and B.empresa = almcajas.empresa and
		  ( (planificacion.tipo_operacion = '31') or (planificacion.tipo_operacion = '9') or (planificacion.tipo_operacion = '27') )
ORDER BY almcajas.descripcion ASC 

  
