 retrieve="  SELECT articulos.descripcion,   
			articulos.control_stock,
			articulos.codigo_pantallas,
			articulos.uso,
         formatos.abreviado,   
         formatos.descripcion,   
         art_ver_tipooperacion.descripcion_abr,   
         planificacion_promocion.empresa,   
         planificacion_promocion.pedido,   
         planificacion_promocion.linped,   
         planificacion_promocion.anyo,   
         planificacion_promocion.orden,   
         planificacion_promocion.lineaprod,   
         planificacion_promocion.ot,   
         planificacion_promocion.ordenoperacion,   
         planificacion_promocion.tipo_operacion,   
         planificacion_promocion.stockacabado,   
         planificacion_promocion.stockbase,   
         planificacion_promocion.stock_cajas,   
         planificacion_promocion.cantidad,   
         planificacion_promocion.formato,   
         planificacion_promocion.cod_articulo,   
         planificacion_promocion.version,   
         planificacion_promocion.base,   
         planificacion_promocion.formato_base,   
         planificacion_promocion.f_entrega,  
         venliped.fentrega_inicial,  
         venliped.fecha_entrega2,  
         venliped.fecha_entrega3,  
         venliped.fecha_entrega4,  
         venliped.fecha_entrega5,  
         planificacion_promocion.usuario,   
         planificacion_promocion.ordenmarcado,   
         planificacion_promocion.velocidad,   
         planificacion_promocion.fecha_fin,   
         planificacion_promocion.hay_obs_ubicaciones,   
         planificacion_promocion.fecha_exacta,   
         articulos.molde,   
         art_codestadistico.TM,   
         planificacion_promocion.fecha_anterior,   
         planificacion_promocion.fecha_sugerida,   
         venliped.fpedido,   
         art_ver_operaciones.numaplicaciones,   
         venliped.fentrega,   
         formatos.descripcion,   
			formatos.ancho,
			formatos.largo,
         venliped.cantidad,   
         genter.razon,   
         venliped.deposito,   
         planificacion_promocion.fecha_salida,   
         planificacion_promocion.ajuste,   
         articulos.bloqueado,
			articulos.molde,
			articulos.marco,
			articulos.plato_sup,
			articulos.plato_inf,
			planificacion_promocion.art_encajado,
			art_tipomoldura.descripcion,
			venliped.planificado,
			planificacion_promocion.f_fin_estimada, 
			ISNULL(CONVERT(char(20), (SELECT MIN (venped.fecha_carga_prevista) FROM venped INNER JOIN venliped ON venped.empresa = venliped.empresa AND venped.anyo = venliped.anyo AND venped.pedido = venliped.pedido WHERE venliped.empresa = planificacion_promocion.empresa AND venliped.articulo = planificacion_promocion.cod_articulo ), 3), '')  AS fecha_carga_prevista_min,
			ISNULL((SELECT case when articulos.unidad = '1' then ROUND(sum(venliped.cantidad) / ((formatos.ancho * formatos.largo) / 10000),0) else sum(venliped.cantidad) end FROM venliped INNER JOIN venped ON venliped.empresa = venped.empresa AND venliped.anyo = venped.anyo AND venliped.pedido = venped.pedido INNER JOIN articulos ON venliped.empresa = articulos.empresa AND venliped.articulo = articulos.codigo INNER JOIN formatos ON articulos.empresa = formatos.empresa AND articulos.formato = formatos.codigo WHERE venliped.empresa = planificacion_promocion.empresa AND venliped.articulo = planificacion_promocion.cod_articulo and venliped.calidad = '1' and venped.es_proforma = 'N' group by articulos.unidad,formatos.ancho,formatos.largo), 0)  AS pedidos_totales,
			ISNULL((SELECT sum( almacenmovimientos.cantidade - almacenmovimientos.cantidads ) FROM almacenmovimientos WHERE almacenmovimientos.empresa = planificacion_promocion.empresa AND almacenmovimientos.articulo = planificacion_promocion.cod_articulo AND almacenmovimientos.calidad = '1'),0) AS STOCK,
			isnull((select sum(planificacion_con_fecha.cantidad) from planificacion_promocion as planificacion_con_fecha where planificacion_con_fecha.empresa = planificacion_promocion.empresa and planificacion_con_fecha.cod_articulo = planificacion_promocion.cod_articulo and planificacion_con_fecha.f_fin_estimada is not null ),0) as planificado_con_fecha,
			articulos.familia,
			planificacion_promocion.lote,
			planificacion_promocion.calibre,
			planificacion_promocion.cod_caja,
			planificacion_promocion.cod_pallet,
			almartcajas.caja,
			almartcajas.piezascaja,
			vista_cajas.codigo_ventas,
			costilla.descripcion,
			( SELECT max (comlinpedpro.fecha_entrega) FROM comlinpedpro WHERE comlinpedpro.empresa = vista_cajas.empresa AND  comlinpedpro.mprima = vista_cajas.codigo_compras ) as fecha_entrega_cajas
		
FROM planificacion_promocion	
LEFT OUTER JOIN art_ver_tipooperacion ON planificacion_promocion.empresa = art_ver_tipooperacion.empresa AND planificacion_promocion.tipo_operacion = art_ver_tipooperacion.codigo 
LEFT OUTER JOIN venliped ON planificacion_promocion.empresa = venliped.empresa AND planificacion_promocion.pedido = venliped.pedido AND planificacion_promocion.linped = venliped.linea AND planificacion_promocion.anyo = venliped.anyo 
LEFT OUTER JOIN almartcajas ON planificacion_promocion.empresa = almartcajas.empresa AND planificacion_promocion.art_encajado = almartcajas.articulo AND almartcajas.pordefecto = 'S'
LEFT OUTER JOIN vista_cajas ON vista_cajas.empresa = almartcajas.empresa AND vista_cajas.codigo_almacen = almartcajas.caja
LEFT OUTER JOIN venped ON planificacion_promocion.empresa = venped.empresa AND planificacion_promocion.pedido = venped.pedido AND planificacion_promocion.anyo = venped.anyo 
LEFT OUTER JOIN art_ver_operaciones ON   planificacion_promocion.empresa = art_ver_operaciones.empresa  and  planificacion_promocion.cod_articulo = art_ver_operaciones.articulo  and planificacion_promocion.version = art_ver_operaciones.version  and  planificacion_promocion.ordenoperacion = art_ver_operaciones.orden 
LEFT OUTER JOIN formatos ON art_ver_operaciones.empresa = formatos.empresa and  art_ver_operaciones.formato = formatos.codigo , articulos 
LEFT OUTER JOIN art_tipomoldura ON articulos.empresa = art_tipomoldura.empresa AND articulos.relieve = art_tipomoldura.codigo
LEFT OUTER JOIN genter ON articulos.empresa = genter.empresa AND articulos.cliente = genter.codigo
LEFT OUTER JOIN costilla ON articulos.empresa = costilla.empresa AND articulos.costilla = costilla.codigo, art_codestadistico  
WHERE 	( articulos.empresa = art_codestadistico.empresa ) and  
         ( articulos.codigo = art_codestadistico.codigo ) and  
         ( planificacion_promocion.empresa = articulos.empresa ) and  
         ( planificacion_promocion.cod_articulo = articulos.codigo ) and  
         ( ( planificacion_promocion.empresa = :empresa ) AND  
         ( planificacion_promocion.lineaprod = :lineaprod ) AND  
         ( genter.tipoter = ~"C~" or genter.tipoter is null) )   
ORDER BY planificacion_promocion.orden ASC   
" arguments=(("empresa", string),("lineaprod", string)) )
