 retrieve="SELECT DISTINCT  planificacion.cod_articulo ,
           planificacion.version ,
		   case when isnull(planificacion.es_hueco,0) = 1 and venliped.descripcion is not null and venliped.descripcion <> '' then venliped.descripcion
		   else articulos.descripcion 
		   end as articulos_descripcion,
           articulos.descripcion ,
           planificacion.formato ,
           formatos.descripcion ,
           art_ver_tipooperacion.descripcion_abr ,
           planificacion.ordenoperacion ,
           planificacion.ot ,
           prodlineas.resumido ,
           planificacion.nivel ,
           planificacion.cantidad ,
           articulos.control_stock ,
           articulos.stocksugerido , 
		   art_ver_operaciones.juegopantallas,
		   planificacion.es_hueco,
		   venliped.descripcion
        FROM planificacion LEFT OUTER JOIN art_ver_operaciones ON planificacion.empresa = art_ver_operaciones.empresa AND 
									planificacion.cod_articulo = art_ver_operaciones.articulo AND planificacion.ordenoperacion = art_ver_operaciones.orden 
									AND planificacion.version = art_ver_operaciones.version
							LEFT OUTER JOIN venliped ON planificacion.empresa = venliped.empresa AND planificacion.anyo = venliped.anyo AND
							planificacion.pedido = venliped.pedido and planificacion.linped = venliped.linea,
           articulos,
           art_ver_tipooperacion ,
           formatos ,
           prodlineas     
        WHERE ( planificacion.empresa = articulos.empresa ) and         
			( planificacion.cod_articulo = articulos.codigo ) and          
			( planificacion.empresa = art_ver_tipooperacion.empresa ) and          
			( planificacion.tipo_operacion = art_ver_tipooperacion.codigo ) and        
			( planificacion.empresa = formatos.empresa ) and          
			( planificacion.formato = formatos.codigo ) and          
			( planificacion.empresa = prodlineas.empresa ) and          
			( planificacion.lineaprod = prodlineas.codigo ) and          
			( ( planificacion.empresa = :empresa) and         
			( planificacion.anyo = :anyo) and          
			( planificacion.pedido =  :pedido) and          
			( planificacion.linped = :linped ) )  


