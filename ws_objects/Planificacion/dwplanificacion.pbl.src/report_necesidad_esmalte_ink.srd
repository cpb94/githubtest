table(column=(type=char(5) updatewhereclause=yes name=planificacion_empresa dbname="planificacion.empresa" )
 column=(type=char(10) updatewhereclause=yes name=art_ver_op_aplicaciones_formula dbname="art_ver_op_aplicaciones.formula" )
 column=(type=char(50) updatewhereclause=yes name=prodformula_descripcion_produccion dbname="prodformula.descripcion_produccion" )
 column=(type=decimal(3) updatewhereclause=yes identity=yes name=gramaje dbname="GRAMAJE" )
 retrieve="  SELECT planificacion.empresa, 
		art_ver_op_aplicaciones.formula, 
		prodformula.descripcion_produccion,
		sum (planificacion.cantidad * art_ver_op_aplicaciones.gramaje) as GRAMAJE
    FROM planificacion, art_ver_op_aplicaciones, prodformula, art_ver_operaciones, prodlineas
   WHERE  planificacion.empresa = :empresa and
		planificacion.cod_articulo = art_ver_operaciones.articulo and  
		planificacion.version = art_ver_operaciones.version and
		planificacion.ordenoperacion = art_ver_operaciones.orden and
		art_ver_operaciones.empresa = art_ver_op_aplicaciones.empresa and
		art_ver_operaciones.articulo = art_ver_op_aplicaciones.articulo and  
		art_ver_operaciones.operacion = art_ver_op_aplicaciones.operacion and  
		art_ver_op_aplicaciones.empresa = prodformula.empresa and  
		art_ver_op_aplicaciones.formula = prodformula.formula and  
		( art_ver_op_aplicaciones.tipoaplicacion = '1' or art_ver_op_aplicaciones.tipoaplicacion = '2' or art_ver_op_aplicaciones.tipoaplicacion = '24') and
		planificacion.empresa = prodlineas.empresa and
		planificacion.lineaprod = prodlineas.codigo 
//and		prodlineas.seccion = '10'
	
GROUP BY planificacion.empresa,
		art_ver_op_aplicaciones.formula, 
		prodformula.descripcion_produccion;

" arguments=(("empresa", string)) )
