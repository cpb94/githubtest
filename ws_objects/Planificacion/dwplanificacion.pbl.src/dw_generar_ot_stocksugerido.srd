table(column=(type=char(5) updatewhereclause=yes name=articulos_empresa dbname="articulos.empresa" )
 column=(type=char(20) updatewhereclause=yes name=articulos_codigo dbname="articulos.codigo" )
 column=(type=char(140) updatewhereclause=yes name=articulos_descripcion dbname="articulos.descripcion" )
 column=(type=char(5) updatewhereclause=yes name=articulos_formato dbname="articulos.formato" )
 column=(type=char(30) updatewhereclause=yes name=formatos_descripcion dbname="formatos.descripcion" )
 column=(type=decimal(0) updatewhereclause=yes name=articulos_stock_min dbname="articulos.stock_min" )
 column=(type=decimal(0) updatewhereclause=yes name=articulos_stocksugerido dbname="articulos.stocksugerido" )
 column=(type=datetime updatewhereclause=yes name=articulos_fechastocksugerido dbname="articulos.fechastocksugerido" )
 column=(type=decimal(6) updatewhereclause=yes name=formatos_largo dbname="formatos.largo" )
 column=(type=decimal(6) updatewhereclause=yes name=formatos_ancho dbname="formatos.ancho" )
 column=(type=decimal(6) updatewhereclause=yes name=venlintarifas_precio dbname="venlintarifas.precio" )
 column=(type=decimal(0) updatewhereclause=yes identity=yes name=planificado dbname="Planificado" )
 column=(type=decimal(6) updatewhereclause=yes identity=yes name=stock_real dbname="stock_real" )
 column=(type=decimal(6) updatewhereclause=yes identity=yes name=cantidad_pedido dbname="cantidad_pedido" )
 column=(type=char(1) updatewhereclause=yes name=marcado dbname="marcado" values="	S/	N" )
 column=(type=char(2) updatewhereclause=yes name=art_codestadistico_g4 dbname="art_codestadistico.G4" )
 column=(type=char(1) updatewhereclause=yes name=articulos_control_stock dbname="articulos.control_stock" )
 retrieve="  SELECT articulos.empresa,   
         articulos.codigo,   
         articulos.descripcion,   
         articulos.formato,   
         formatos.descripcion, 
		articulos.stock_min,     
         articulos.stocksugerido,   
         articulos.fechastocksugerido, 
         formatos.largo,   
         formatos.ancho,   
         venlintarifas.precio,  
		 (select sum(planificacion.cantidad) from planificacion where planificacion.empresa = :arg_empresa and planificacion.cod_articulo = articulos.codigo and planificacion.pedido = 0 ) as Planificado,
         sum(almacenmovimientos.cantidade - cantidads) stock_real,   
         (select sum(venliped.cantidad) from venliped where empresa = :arg_empresa and articulo = articulos.codigo ) as cantidad_pedido,   
         'N' marcado,   
         art_codestadistico.G4,  
		articulos.control_stock				  

    FROM articulos,   
         formatos,   
         almacenmovimientos,   
         venlintarifas,   
         art_codestadistico,
		planificacion
   WHERE ( formatos.empresa *= almacenmovimientos.empresa) and  
         ( articulos.codigo *= almacenmovimientos.articulo) and  
/*         ( ( almacenmovimientos.zona <> 'P') or  ( almacenmovimientos.almacen <> '1' ) ) and  */
		( articulos.empresa *= planificacion.empresa ) and
		(articulos.codigo *= planificacion.cod_articulo ) and
         ( articulos.empresa *= venlintarifas.empresa) and  
         ( articulos.codigo *= venlintarifas.articulo) and  
         ( articulos.empresa = art_codestadistico.empresa ) and  
         ( articulos.codigo = art_codestadistico.codigo ) and  
         ( articulos.empresa = formatos.empresa ) and  
         ( articulos.formato = formatos.codigo ) and  
         ( ( articulos.empresa = :arg_empresa ) AND  
         ( articulos.cliente like :cliente ) AND  
         ( formatos.ancho >= :ancho1 ) AND  
         ( formatos.ancho <= :ancho2 ) AND  
         ( formatos.largo >= :largo1 ) AND  
         ( formatos.largo <= :largo2 ) AND  
         ( articulos.stocksugerido <> 0 ) AND  
         ( articulos.stocksugerido is not null ) AND  
         ( articulos.codigo like :articulo ) ) AND  		
		venlintarifas.tarifa = (select max(b.tarifa) from venlintarifas b where b.empresa = :arg_empresa and b.articulo = articulos.codigo and b.tarifa <> '5') and
         art_codestadistico.G4 like :g4 AND  		
         articulos.relieve like :tm AND
		articulos.familia like :familia  
GROUP BY articulos.empresa,   
         articulos.codigo,   
         articulos.descripcion,   
         articulos.formato,   
         formatos.descripcion, 
		articulos.stock_min,       
         articulos.stocksugerido,   
         articulos.fechastocksugerido,   
         formatos.largo,   
         formatos.ancho,   
         art_codestadistico.G4,   
		articulos.control_stock,
         venlintarifas.precio   



/*
  SELECT articulos.empresa,   
         articulos.codigo,   
         articulos.descripcion,   
         articulos.formato,   
         formatos.descripcion,   
         articulos.stocksugerido,   
         articulos.fechastocksugerido, 
         formatos.largo,   
         formatos.ancho,   
         venlintarifas.precio,  		 
         sum(almacenmovimientos.cantidade - cantidads) stock_real,   
         (select sum(venliped.cantidad) from venliped where empresa = :arg_empresa and articulo = articulos.codigo) as cantidad_pedido,   
         'N' marcado,   
         art_codestadistico.G4,  
			articulos.control_stock				  

    FROM articulos,   
         formatos,   
         almacenmovimientos,   
         venlintarifas,   
         art_codestadistico
   WHERE ( formatos.empresa *= almacenmovimientos.empresa) and  
         ( articulos.codigo *= almacenmovimientos.articulo) and  
         ( articulos.empresa *= venlintarifas.empresa) and  
         ( articulos.codigo *= venlintarifas.articulo) and  
         ( articulos.empresa = art_codestadistico.empresa ) and  
         ( articulos.codigo = art_codestadistico.codigo ) and  
         ( articulos.empresa = formatos.empresa ) and  
         ( articulos.formato = formatos.codigo ) and  
         ( ( articulos.empresa = :arg_empresa ) AND  
         ( articulos.cliente like :cliente ) AND  
         ( formatos.ancho >= :ancho1 ) AND  
         ( formatos.ancho <= :ancho2 ) AND  
         ( formatos.largo >= :largo1 ) AND  
         ( formatos.largo <= :largo2 ) AND  
         ( articulos.stocksugerido <> 0 ) AND  
         ( articulos.stocksugerido is not null ) AND  
         ( articulos.codigo like :articulo ) ) AND  		
			venlintarifas.tarifa = (select max(b.tarifa) from venlintarifas b where b.empresa = :arg_empresa and b.articulo = articulos.codigo and b.tarifa <> '5') and
         art_codestadistico.G4 like :g4 AND  		
         articulos.relieve like :tm AND
		articulos.familia like :familia  
GROUP BY articulos.empresa,   
         articulos.codigo,   
         articulos.descripcion,   
         articulos.formato,   
         formatos.descripcion,   
         articulos.stocksugerido,   
         articulos.fechastocksugerido,   
         formatos.largo,   
         formatos.ancho,   
         art_codestadistico.G4,   
			articulos.control_stock,
         venlintarifas.precio   

*/
" retrieve.asneeded=yes arguments=(("arg_empresa", string),("cliente", string),("articulo", string),("g4", string),("tm", string),("ancho1", number),("ancho2", number),("largo1", number),("largo2", number),("familia", string))  sort="sugerencia D articulos_descripcion A articulos_formato A " )
