global function string f_prefijo_pais (string ra_codpais);string nombre

  SELECT paises.prefijo
    INTO :nombre
    FROM paises  
   WHERE paises.pais = :ra_codpais  
 USING SQLCA;

IF IsNull(nombre) Then nombre = ""
RETURN nombre







end function

