global function string f_tipo_pais (string ra_codpais);string nombre

  SELECT paises.tipopais
    INTO :nombre
    FROM paises  
   WHERE paises.pais = :ra_codpais
   ORDER BY paises.pais ASC USING SQLCA;

IF IsNull(nombre) Then nombre = ""

RETURN nombre







end function

