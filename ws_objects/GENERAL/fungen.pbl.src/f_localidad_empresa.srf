global function string f_localidad_empresa (string ai_codigo_empresa);string pais

	SELECT empresas.ciudad INTO :pais  FROM empresas  
	WHERE  empresas.empresa = :ai_codigo_empresa;
	IF IsNull(pais) Then pais = "" 
	
RETURN pais
end function

