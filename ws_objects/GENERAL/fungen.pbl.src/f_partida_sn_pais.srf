global function string f_partida_sn_pais (string pais);String var_valor

  SELECT paises.nom_partida
    INTO :var_valor  
    FROM paises  
   WHERE paises.pais = :pais;
	If IsNull(var_valor) Then var_valor = ""
Return var_valor

end function

