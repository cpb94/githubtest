 retrieve="PBSELECT( VERSION(400) TABLE(NAME=~"venfac~" )  TABLE(NAME=~"monedas~" ) COLUMN(NAME=~"venfac.divisa~") COLUMN(NAME=~"venfac.anyo~") COLUMN(NAME=~"venfac.factura~") COLUMN(NAME=~"venfac.ffactura~") COLUMN(NAME=~"venfac.empresa~") COLUMN(NAME=~"venfac.total_neto~") COLUMN(NAME=~"monedas.abreviado~") COLUMN(NAME=~"venfac.dtoesp~") COLUMN(NAME=~"venfac.total_fac_pts~") COMPUTE(NAME=~"~~~"                                                   ~~~" nombre_cliente~") COLUMN(NAME=~"venfac.cliente~") COLUMN(NAME=~"venfac.total_fac~") COMPUTE(NAME=~"~~~"S~~~" marca~") COMPUTE(NAME=~"0.00 pendiente~") COMPUTE(NAME=~"0.00 impdto~")    JOIN (LEFT=~"venfac.divisa~"    OP =~"=~"RIGHT=~"monedas.moneda~"    OUTER1 =~"venfac.divisa~" )WHERE(    EXP1 =~"venfac.empresa~"   OP =~"=~"    EXP2 =~":empresa~"    LOGIC =~"and~" ) WHERE(    EXP1 =~"venfac.cliente~"   OP =~"between~"    EXP2 =~":cli1  and :cli2~"    LOGIC =~"and~" ) WHERE(    EXP1 =~"venfac.ffactura~"   OP =~">=~"    EXP2 =~":fdesde~"    LOGIC =~"and~" ) WHERE(    EXP1 =~"venfac.ffactura~"   OP =~"<=~"    EXP2 =~":fhasta~"    LOGIC =~"and~" ) WHERE(    EXP1 =~"venfac.dtoesp~"   OP =~"<>~"    EXP2 =~"0~"    LOGIC =~"and~" ) WHERE(    EXP1 =~"venfac.actualizada~"   OP =~"=~"    EXP2 =~"'S'~"    LOGIC =~"and~" ) WHERE(    EXP1 =~"venfac.marcada~"   OP =~"=~"    EXP2 =~"'N'~" ) ) ORDER(NAME=~"venfac.ffactura~" ASC=yes )  ORDER(NAME=~"venfac.factura~" ASC=yes ) ARG(NAME = ~"empresa~" TYPE = string)  ARG(NAME = ~"cli1~" TYPE = string)  ARG(NAME = ~"cli2~" TYPE = string)  ARG(NAME = ~"fdesde~" TYPE = datetime)  ARG(NAME = ~"fhasta~" TYPE = datetime) " filter=" venfac_total_fac <> pendiente and marca = ~"S~"" arguments=(("empresa", string),("cli1", string),("cli2", string),("fdesde", datetime),("fhasta", datetime)) )
