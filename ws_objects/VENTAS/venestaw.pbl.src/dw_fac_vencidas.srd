 retrieve="PBSELECT( VERSION(400) TABLE(NAME=~"carefectos~" ) COLUMN(NAME=~"carefectos.empresa~") COLUMN(NAME=~"carefectos.anyo~") COLUMN(NAME=~"carefectos.orden~") COLUMN(NAME=~"carefectos.anyofra~") COLUMN(NAME=~"carefectos.factura~") COLUMN(NAME=~"carefectos.recibo~") COLUMN(NAME=~"carefectos.fvto~") COLUMN(NAME=~"carefectos.ffra~") COLUMN(NAME=~"carefectos.importe~") COMPUTE(NAME=~"convert(int,(datediff(day ,carefectos.fvto , getdate())/30)) tipo~")WHERE(    EXP1 =~"carefectos.empresa~"   OP =~"=~"    EXP2 =~":arg_emp~"    LOGIC =~"and~" ) WHERE(    EXP1 =~"carefectos.cliente~"   OP =~"=~"    EXP2 =~":arg_cli~"    LOGIC =~"and~" ) WHERE(    EXP1 =~"carefectos.situacion~"   OP =~"in~"    EXP2 =~"('0','4','1','5','6','10')~"    LOGIC =~"and~" ) WHERE(    EXP1 =~"carefectos.fvto~"   OP =~"<~"    EXP2 =~"getdate()~" ) ) ORDER(NAME=~"carefectos.fvto~" ASC=no) ARG(NAME = ~"arg_emp~" TYPE = string)  ARG(NAME = ~"arg_cli~" TYPE = string) " update="carefectos" updatewhere=1 updatekeyinplace=no arguments=(("arg_emp", string),("arg_cli", string)) )
