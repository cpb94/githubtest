         venlintarifas.articulo,   
         venlintarifas.calidad,   
         venlintarifas.precio,   
         venlintarifas.falta,   
         venlintarifas.montajeartcal,   
         venlintarifas.familia,   
         venlintarifas.formato,   
         venlintarifas.modelo,   
         venlintarifas.activo,   
         venlintarifas.listar,   
         venlintarifas.usuario,   
         venlintarifas.descripcion,   
         venlintarifas.excepcion,   
         venlintarifas.tipo_linea,   
         venlintarifas.sub_familia,
		articulos.descripcion,
		formatos.descripcion   
    FROM venlintarifas  
LEFT OUTER JOIN articulos ON venlintarifas.empresa = articulos.empresa AND venlintarifas.articulo  = articulos.codigo 
LEFT OUTER JOIN formatos ON venlintarifas.empresa = formatos.empresa AND venlintarifas.formato  = formatos.codigo 
   WHERE ( venlintarifas.empresa = :arg_empresa ) AND  
         ( venlintarifas.tarifa = :arg_tarifa ) AND  
         ( venlintarifas.fecha = :arg_fecha ) AND  
         ( venlintarifas.listar = ~"S~" ) 
order by convert (integer,venlintarifas.articulo)    

