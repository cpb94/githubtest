public subroutine f_recalcular_lineas ();str_venlialb2  linalb2
str_venalb2   ped 
Integer  reg,reg1 
Dec{0}  ultima,albaran,periodo,parcial_pallets
Dec{2}  parcial_cantidad,parcial_cajas
Dec{4}  impdto,importe,acumulado,importedto1,importedto2,importedto3,importedto4,importedtopp,importedtoesp,control_precio,control_bruto,control_importedto,control_importe
Dec{2}  var_dto1,var_dto2,var_dto3,var_dto4,var_dtopp,control_descuento
String  var_tipodto1,var_tipodto2,var_tipodto3,var_tipodto4,var_calculo_dto
Dec{4}  var_impdto1,var_impdto2,var_impdto3,var_impdto4
Integer procesos

periodo   =  Integer(em_anyo.text)
albaran    =  Dec(em_albaran.text)
reg1 = dw_detalle.RowCount()
linalb2.empresa   = codigo_empresa
linalb2.anyo      = periodo
linalb2.albaran  = albaran
  SELECT venalb2.dtoesp1,     venalb2.dtoesp2,   
         venalb2.dtoesp3,     venalb2.dtoesp4,   
         venalb2.dtopp,       venalb2.tipodto1,   
         venalb2.tipodto2,    venalb2.tipodto3,   
         venalb2.tipodto4,    venalb2.divisa,
			venalb2.calculo_dto
    INTO :var_dto1,    :var_dto2,    :var_dto3,  :var_dto4,   
         :var_dtopp,   :var_tipodto1,:var_tipodto2,   
         :var_tipodto3,:var_tipodto4,:linalb2.divisa,
			:var_calculo_dto
    FROM venalb2  
   WHERE (venalb2.empresa  = :linalb2.empresa ) AND  
         (venalb2.anyo     = :linalb2.anyo ) AND  
         (venalb2.albaran = :linalb2.albaran );


For reg = 1 To reg1 
 linalb2.linea              = Dec(f_valor_columna(dw_detalle,reg,"linea"))
 linalb2.cantidad           = Dec(f_valor_columna(dw_detalle,reg,"cantidad"))
 linalb2.dtoesp             = Dec(f_valor_columna(dw_detalle,reg,"dtoesp"))
 linalb2.tipolinea         = f_valor_columna(dw_detalle,reg,"tipolinea")
 linalb2.control_comisiones = f_comision_ventipolin(linalb2.empresa,linalb2.tipolinea)
 linalb2.control_descuentos = f_descuento_ventipolin(linalb2.empresa,linalb2.tipolinea)

 IF  IsNull(linalb2.dtoesp) Then linalb2.dtoesp= 0
 IF IsNull(var_dto1)  Then var_dto1  = 0
 IF IsNull(var_dto2)  Then var_dto2  = 0
 IF IsNull(var_dto3)  Then var_dto3  = 0
 IF IsNull(var_dto4)  Then var_dto4  = 0
 IF IsNull(var_dtopp) Then var_dtopp = 0

For procesos = 1 To 2
 linalb2.precio_aduana      = Dec(f_valor_columna(dw_detalle,reg,"precio_aduana"))
 linalb2.precio             = Dec(f_valor_columna(dw_detalle,reg,"precio"))
 IF procesos = 1 then control_precio = linalb2.precio
 IF procesos = 2 then control_precio = linalb2.precio_aduana
	
	control_bruto = Dec(String(linalb2.cantidad * control_precio,f_mascara_moneda(linalb2.divisa)))
	IF var_calculo_dto = "N" Then
		importedtoesp = Dec(String((control_bruto * linalb2.dtoesp /100),f_mascara_moneda(linalb2.divisa)))
		acumulado = control_bruto - importedtoesp
	ELSE
		acumulado = control_bruto
	END IF
	
	IF linalb2.control_descuentos = "N" Then
		linalb2.descuento = 0
	ELSE
	 IF var_tipodto1  = "L" Then 
			importedto1 = Dec(String((acumulado * var_dto1 /100),f_mascara_moneda(linalb2.divisa)))
			acumulado = acumulado - importedto1
	 END IF
	 IF var_tipodto2  = "L" Then 
			importedto2 = Dec(String((acumulado * var_dto2 /100),f_mascara_moneda(linalb2.divisa)))
			acumulado = acumulado - importedto2
	 END IF
	 IF var_tipodto3  = "L" Then 	
		   importedto3 = Dec(String((acumulado * var_dto3 /100),f_mascara_moneda(linalb2.divisa)))
			acumulado = acumulado - importedto3
	 END IF
	 IF var_tipodto4  = "L" Then 	
			importedto4  = Dec(String((acumulado * var_dto4 /100),f_mascara_moneda(linalb2.divisa)))
			acumulado = acumulado - importedto4
	 END IF
	 importedtopp  = Dec(String((acumulado * var_dtopp /100),f_mascara_moneda(linalb2.divisa))) 
	END IF
	control_importedto    = control_bruto - acumulado 
	control_importe       = control_bruto - control_importedto
	If control_bruto = 0 then
		control_descuento  = 0
	ELse
		control_descuento  = (control_importedto/control_bruto)* 100
	END IF
	control_importedto    = control_bruto * control_descuento / 100
	control_importe       = control_bruto - control_importedto
	
	IF var_calculo_dto = "S" Then
		control_precio = control_precio - &
		Dec(String((control_precio * control_descuento)/100,f_mascara_moneda(linalb2.divisa)))
		control_descuento = linalb2.dtoesp
		control_bruto = Dec(String(linalb2.cantidad * control_precio,f_mascara_moneda(linalb2.divisa)))
	   importedtoesp = Dec(String((control_bruto * linalb2.dtoesp /100),f_mascara_moneda(linalb2.divisa)))
		control_importedto    = importedtoesp
	   control_importe       = control_bruto - importedtoesp
	END IF
	
	IF procesos = 1 Then
		linalb2.descuento       = control_descuento
		linalb2.bruto           = control_bruto
	   linalb2.importedto      = control_importedto
	   linalb2.importe         = control_importe
		linalb2.precio_neto     = control_precio
		
	END IF
	IF procesos = 2 Then
		linalb2.descuento_aduana       = control_descuento
	   linalb2.importe_aduana         = control_importe
		linalb2.precio_aduana_neto     = control_precio
	END IF
Next
  UPDATE venlialb2  
     SET importe            = :linalb2.importe,   
         descuento          = :linalb2.descuento,   
         importedto         = :linalb2.importedto,   
         precio_neto        = :linalb2.precio_neto,   
         precio_aduana_neto = :linalb2.precio_aduana_neto,   
         descuento_aduana   = :linalb2.descuento_aduana,   
         importe_aduana     = :linalb2.importe_aduana,   
         bruto = :linalb2.bruto  
   WHERE ( venlialb2.empresa = :linalb2.empresa ) AND  
         ( venlialb2.anyo = :linalb2.anyo ) AND  
         ( venlialb2.albaran = :linalb2.albaran )  AND 
        ( venlialb2.linea = :linalb2.linea )   ;
COMMIT;
NEXT
f_actualizar_venalbaran2(codigo_empresa,periodo,albaran)
end subroutine

