event clicked;call super::clicked;Dec cj,tcj,vanyo,valbaran,vlinea
IF dw_detalle.GetRow() <> 0 Then
IF f_control_almacen_ventipolin(codigo_empresa,dw_detalle.GetItemString(dw_detalle.GetRow(),"tipolinea")) = "N" Then Return
	cj     = dw_detalle.GetItemNumber(dw_detalle.GetRow(),"venliab2_cajas")
	tcj     = dw_detalle.GetItemNumber(dw_detalle.GetRow(),"venliab2_total_cajas")
	vanyo    = dw_detalle.GetItemNumber(dw_detalle.GetRow(),"venliab2_anyo")
	valbaran = dw_detalle.GetItemNumber(dw_detalle.GetRow(),"venliab2_albaran")
	vlinea   = dw_detalle.GetItemNumber(dw_detalle.GetRow(),"venliab2_linea")
	IF IsNull(cj) Then cj = 0
	cj = cj +1
	IF IsNull(tcj) Then tcj = 0
	tcj = tcj +1
	dw_detalle.SetItem(dw_detalle.GetRow(),"venliab2_cajas",cj)
	dw_detalle.SetItem(dw_detalle.GetRow(),"venliab2_total_cajas",tcj)
	Update venlialb2
	Set cajas = :cj,
	    total_cajas = :tcj
	Where empresa = :codigo_empresa
	And   anyo    = :vanyo
	And   albaran = :valbaran
	And   linea   = :vlinea;
	Commit;
END IF
end event

