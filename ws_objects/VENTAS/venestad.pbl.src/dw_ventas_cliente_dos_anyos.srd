table(column=(type=char(15) updatewhereclause=yes name=venlifac_cliente dbname="venlifac.cliente" dbalias=".cliente" )
 column=(type=char(50) updatewhereclause=yes name=genter_razon dbname="genter.razon" dbalias=".razon" )
 column=(type=decimal(20) updatewhereclause=yes identity=yes name=ventas_anterior dbname="ventas_anterior" )
 column=(type=decimal(20) updatewhereclause=yes identity=yes name=ventas_actual dbname="ventas_actual" )
 retrieve="select venlifac.cliente
 , genter.razon
 , sum(case when venfac.ffactura between  CONVERT (datetime, '01-01-'+CAST (year(getdate() )-1  AS VARCHAR(4)), 110) and dateadd ( year, -1, getdate()) 
 		then (venlifac.neto - venlifac.impdtopp) / venfac.cambio 
 		else 0  
 	end ) as ventas_anterior 
 , sum(case when venfac.ffactura between  CONVERT (datetime, '01-01-'+CAST (year(getdate() )  AS VARCHAR(4)), 110) and  getdate() 
 		then (venlifac.neto - venlifac.impdtopp) / venfac.cambio 
 		else 0  
 	end ) as ventas_actual 
 from venlifac 
 inner join venfac on venlifac.empresa = venfac.empresa and venlifac.anyo = venfac.anyo and venlifac.factura = venfac.factura  
 inner join articulos on venlifac.empresa = articulos.empresa and venlifac.articulo = articulos.codigo 
 inner join genter on venlifac.empresa = genter.empresa and venlifac.cliente = genter.codigo
 where articulos.familia like :familia
and genter.pais like :pais
and venlifac.empresa  = :empresa
 and genter.tipoter = 'C'
 and articulos.promocion <> 'S'
 group by venlifac.cliente, genter.razon
order by  ventas_actual desc " arguments=(("empresa", string),("familia", string),("pais", string)) )
