table(column=(type=long updatewhereclause=yes name=prev_familias_mes dbname="prev_familias.mes" )
 column=(type=decimal(2) updatewhereclause=yes name=prev_familias_prevision dbname="prev_familias.prevision" )
 column=(type=decimal(21) updatewhereclause=yes identity=yes name=ventas_mes dbname="ventas_mes" )
 retrieve="  SELECT prev_familias.mes,   prev_familias.prevision,   sum(venlifac.neto   / cambio) as ventas_mes
    FROM venlifac,   
         prev_familias  
   WHERE ( prev_familias.ejercicio *= venlifac.anyo) and  
		prev_familias.mes *= month(venlifac.ffactura) and
          ( venlifac.empresa = :empresa ) AND  
         ( prev_familias.ejercicio = :anyo ) and
         ( prev_familias.familia like :familia ) and
		venlifac.familia like :familia
   
group by  prev_familias.mes,   prev_familias.prevision
order by prev_familias.mes

/*
  SELECT prev_familias.mes,   prev_familias.prevision,   sum(venfac.total_neto   / cambio) as ventas_mes
    FROM venfac,   
         prev_familias  
   WHERE ( prev_familias.ejercicio *= venfac.anyo) and  
		prev_familias.mes *= month(venfac.ffactura) and
          ( venfac.empresa = :empresa ) AND  
         ( prev_familias.ejercicio = :anyo ) and
         ( prev_familias.familia = :familia )    
group by  prev_familias.mes,   prev_familias.prevision
order by prev_familias.mes

*/" arguments=(("empresa", string),("anyo", number),("mes", number),("familia", string)) )
