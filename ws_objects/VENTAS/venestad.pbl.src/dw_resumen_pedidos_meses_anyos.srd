 retrieve="select 	vista_venliped.empresa,   
         vista_venliped.anyo,   
         vista_venliped.pedido,   
         vista_venliped.linea,  
			vista_venliped.fpedido,
         ( vista_venliped.neto - vista_venliped.impdtopp ) / vista_venliped.cambio as importe_moneda,
         vista_venliped.cantidad,
         articulos.fecha_alta,
			formatos.ancho,
			formatos.largo,
			formatos.ancho * formatos.largo * vista_venliped.cantidad / 10000 as metros,
			formatos.largo * vista_venliped.cantidad / 100 as metros_largo

from vista_venliped, articulos, formatos
where vista_venliped.empresa = :empresa 
and articulos.empresa = :empresa
and formatos.empresa = :empresa   
and vista_venliped.articulo = articulos.codigo
and articulos.formato = formatos.codigo 
and ((vista_venliped.fpedido >= :fdesde1 
and vista_venliped.fpedido <= :fhasta1 ) 
or (vista_venliped.fpedido >= :fdesde2
and vista_venliped.fpedido <= :fhasta2))
and vista_venliped.cliente like :cliente
and vista_venliped.familia like :familia
and vista_venliped.pais like :pais
and articulos.promocion <> 'S' 
" arguments=(("empresa", string),("cliente", string),("fdesde1", datetime),("fhasta1", datetime),("fdesde2", datetime),("fhasta2", datetime),("familia", string),("pais", string))  sort="venliped_anyo A " )
