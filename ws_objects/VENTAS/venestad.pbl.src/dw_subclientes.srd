table(column=(type=char(15) updatewhereclause=yes name=articulos_cliente_marca dbname="articulos.cliente_marca" )
 column=(type=char(50) updatewhereclause=yes name=genter_razon dbname="genter.razon" )
 column=(type=decimal(21) updatewhereclause=yes identity=yes name=importe dbname="importe" )
 retrieve="

 SELECT articulos.cliente_marca , genter.razon, sum (venlifac.neto / venlifac.cambio ) importe
    FROM articulos,   
         venlifac,   
         genter  
    WHERE ( venlifac.empresa = articulos.empresa ) and  
         ( venlifac.articulo = articulos.codigo ) and  
         ( articulos.empresa = genter.empresa ) and  
         ( articulos.cliente_marca = genter.codigo ) and  
         ( ( articulos.empresa = :empresa ) AND  
         ( venlifac.cliente = :cliente ) AND  
         ( genter.tipoter = 'C' ) AND  
         ( venlifac.ffactura >= :fechad ) AND  
         ( venlifac.ffactura <= :fechah ) )    
         
         group by articulos.cliente_marca , genter.razon " arguments=(("empresa", string),("cliente", string),("fechad", datetime),("fechah", datetime)) )
