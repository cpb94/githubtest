event rowfocuschanged;string coleccion, modelo_nuevo, modelo_viejo, articulo

if currentrow > 0 then
	articulo = dw_1.object.articulos_codigo[currentrow]
	
	This.SelectRow(0, FALSE)	
	This.SelectRow ( currentrow, True )
	
	SELECT codigo_pantallas, modelo_origen
	INTO :modelo_viejo, :modelo_nuevo 
	FROM articulos 
	WHERE empresa = :codigo_empresa AND codigo = :articulo;
	
	p_imagen.PictureName = ""
	IF dw_1.GetRow() > 0 then
		if not isnull(modelo_nuevo) and modelo_nuevo <> "" then
			SELECT coleccion INTO :coleccion FROM modelo WHERE empresa = :codigo_empresa AND modelo = :modelo_nuevo;
			ftc_imagen_fondo_datawindow(dw_imagen,"5",coleccion,modelo_nuevo,"")	
		else
			p_imagen.PictureName = f_cargar_imagen_nuevo(string(modelo_viejo))
			dw_imagen.visible = false
		end if
	else
		dw_imagen.visible = false
		p_imagen.PictureName = ""
	END IF
end if


