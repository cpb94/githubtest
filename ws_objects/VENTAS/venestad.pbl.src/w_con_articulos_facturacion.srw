sql = "  SELECT articulos.codigo,   "+&
"         articulos.descripcion,   "+&
"         formatos.codigo,   "+&
"         formatos.abreviado,   "+&
"         formatos.ancho,   "+&
"         formatos.largo,   "+&
"         articulos.fecha_alta,   "+&
"         genter.razon, "+&
"         articulos.cliente,   "+&
"         art_codestadistico.codigo,   "+&
"         art_codestadistico.G1,   "+&
"         art_codestadistico.G2,   "+&
"         art_codestadistico.G3,   "+&
"         art_codestadistico.G4,   "+&
"         art_codestadistico.PT,   "+&
"         art_codestadistico.GD,   "+&
"         art_codestadistico.NC,   "+&
"         art_codestadistico.RE,   "+&
"         art_codestadistico.AV,   "+&
"         art_codestadistico.PL,   "+&
"         art_codestadistico.CT,   "+&
"         art_codestadistico.EN,   "+&
"         art_codestadistico.OTR,   "+&
"         art_codestadistico.N,   "+&
"         articulos.relieve, "+&
"		 	 articulos.familia, "+&
"			 articulos.molde, "+&	
" 		isnull (sum (case when venlifac.tipo_unidad  = 0 then venlifac.cantidad else "+&
"		 case when venlifac.tipo_unidad  = 1 then   venlifac.cantidad / ((articulos.ancho_std * articulos.largo_std)  / 10000 ) END END), 0 ) piezas, "+&
"	 	isnull (sum (case when venlifac.tipo_unidad  = 1 then venlifac.cantidad else "+&
"		 case when venlifac.tipo_unidad  = 0 then   venlifac.cantidad * ((articulos.ancho_std * articulos.largo_std)  / 10000 ) END END), 0 ) metros, "+&
"		sum(venlifac.neto  /  venlifac.cambio) as importe, "+&
"		sum (venlifac.total_cajas * almcajas.pesoenvase) AS pesoenvase, "+&
"         articulos.submarcas_codigo "+&
"    FROM articulos "+&
"		LEFT OUTER JOIN formatos ON  articulos.empresa = formatos.empresa and articulos.formato = formatos.codigo "+&
"         LEFT OUTER JOIN genter ON articulos.empresa = genter.empresa and articulos.cliente = genter.codigo and genter.tipoter = 'C' "+&
"         LEFT OUTER JOIN art_codestadistico ON articulos.empresa = art_codestadistico.empresa and articulos.codigo = art_codestadistico.codigo "+&
"         LEFT OUTER JOIN venlifac ON  articulos.empresa = venlifac.empresa and articulos.codigo = venlifac.articulo "+&
"         INNER JOIN almartcajas ON  articulos.empresa = almartcajas.empresa AND articulos.codigo = almartcajas.articulo AND almartcajas.pordefecto = 'S' "+&
"         INNER JOIN almcajas ON  almartcajas.empresa = almcajas.empresa AND almartcajas.caja = almcajas.codigo  "+&
"         WHERE   ( articulos.codigo like '"+articulo+"') AND  "+&
"		( articulos.formato like '"+formato+"' ) AND "+filtro+&
"		articulos.fecha_alta >= '"+String(fecha1_alta, "dd-mm-yyyy")+"'  AND  "+&
"		articulos.fecha_alta <= '"+String(fecha2_alta, "dd-mm-yyyy")+"'  AND  "+&
"		venlifac.ffactura >= '"+String(fecha1, "dd-mm-yyyy")+"'  AND  "+&
"     	venlifac.ffactura <= '"+String(fecha2, "dd-mm-yyyy")+"'  AND  "+&
"		formatos.ancho >= "+String(v_ancho1)+" and "+&
"		formatos.ancho <= "+String(v_ancho2)+" and "+&
"		formatos.largo >= "+String(v_largo1)+" and "+&
"		formatos.largo <= "+String(v_largo2)+"  "+&
"group by articulos.codigo,   "+&
"         articulos.descripcion, "+&  
"         formatos.codigo,   "+&
"         formatos.abreviado,   "+&
"         formatos.ancho,   "+&
"         formatos.largo,   "+&
"         articulos.fecha_alta,   "+&
"         genter.razon, "+& 
"         articulos.cliente,   "+&
"         art_codestadistico.codigo,   "+&
"         art_codestadistico.G1,   "+&
"         art_codestadistico.G2,   "+&
"         art_codestadistico.G3,   "+&
"         art_codestadistico.G4,   "+&
"         art_codestadistico.PT,   "+&
"         art_codestadistico.GD,   "+&
"         art_codestadistico.NC,   "+&
"         art_codestadistico.RE,   "+&
"         art_codestadistico.AV,   "+&
"         art_codestadistico.PL,   "+&
"         art_codestadistico.CT,   "+&
"         art_codestadistico.EN,   "+&
"         art_codestadistico.OTR,   "+&
"         art_codestadistico.N, "+&
"         articulos.relieve, "+&
"		    articulos.familia, "+&
"         articulos.submarcas_codigo, "+&
"			 articulos.molde "


dw_1.SetSQLSelect ( sql )
dw_1.retrieve()	

