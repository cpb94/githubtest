         letras serie,   
         letras cliente,   
         letras nombre_cliente,   
         letras nombre_serie,   
         numerico importe_nacional,   
         numerico importe_cee,   
         numerico importe_export,
			numerico importe_total,   
         letras moneda,
			letras fecha_calculo,
			fecha fecha_ini,
			fecha fecha_fin  
    FROM comodin  
   WHERE comodin.empresa = :empresa    
