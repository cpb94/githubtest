         letras serie,   
         letras cliente,   
         letras nombre_cliente,   
         letras nombre_serie,   
         numerico importe,   
			numerico cantidad,   
			numerico metros,
         letras moneda,
			letras fecha_calculo,
			fecha fecha_ini,
			fecha fecha_fin,
			letras cadena,
			letras grupo,
			letras pais,
			numerico orden
    FROM comodin  
   WHERE comodin.empresa = :empresa    
