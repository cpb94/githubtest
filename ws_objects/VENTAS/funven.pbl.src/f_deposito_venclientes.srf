global function string f_deposito_venclientes (string empresa, string cliente);String valor

SELECT   venclientes.deposito
INTO     :valor
FROM     venclientes  
Where    venclientes.empresa = :empresa
And      venclientes.codigo = :cliente;
Return valor;

end function

