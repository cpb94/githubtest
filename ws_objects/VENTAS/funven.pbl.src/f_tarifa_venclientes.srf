global function string f_tarifa_venclientes (string empresa, string cliente);String valor

SELECT isnull(venclientes.tarifa,'')
INTO   :valor
FROM   venclientes  
Where  venclientes.empresa = :empresa
And    venclientes.codigo  = :cliente;

Return valor;

end function

