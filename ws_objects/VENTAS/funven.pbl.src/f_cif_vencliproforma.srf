global function string f_cif_vencliproforma (string empresa, string codigo);String var

  SELECT vencliproforma.cif
    INTO :var  
    FROM vencliproforma  
   WHERE ( vencliproforma.empresa = :empresa )
	AND   ( vencliproforma.codigo = :codigo )   ;
	Return var

end function

