global function string f_valor1_venpartidas (string empresa, string codigo);String var

   SELECT venpartidas.valor1  
    INTO :var
    FROM venpartidas  
   WHERE ( venpartidas.empresa = :empresa ) AND  
         ( venpartidas.codigo = :codigo )   ;


RETURN var
