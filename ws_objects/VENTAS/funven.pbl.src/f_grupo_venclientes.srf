global function string f_grupo_venclientes (string empresa, string cliente);String valor

SELECT venclientes.grupo
INTO     :valor
FROM     venclientes  
Where    venclientes.empresa = :empresa
And      venclientes.codigo = :cliente;

Return valor;

end function

