global function string f_pais_venlugares (string cod_empresa, string cod);String ls_pais

 SELECT    venlugares.pais
    INTO   :ls_pais
    FROM venlugares 
   WHERE ( venlugares.empresa = :cod_empresa) AND  
         ( venlugares.codigo  = :cod  );

RETURN ls_pais
//

end function

