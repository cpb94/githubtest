global function string f_textopie2_venped (string empresa, integer anyo, decimal pedido);String var


  SELECT venped.textopie2
    INTO :var
    FROM venped  
   WHERE (venped.empresa = :empresa)
   AND   (venped.anyo    = :anyo)
   AND   (venped.pedido  = :pedido)   ;



Return var
end function

