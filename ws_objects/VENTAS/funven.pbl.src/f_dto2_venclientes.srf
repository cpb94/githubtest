global function decimal f_dto2_venclientes (string empresa, string cliente);dec valor

SELECT venclientes.dto2
INTO     :valor
FROM     venclientes  
Where    venclientes.empresa = :empresa
And      venclientes.codigo = :cliente;

Return valor;

end function

