global function string f_serie_venclientes (string empresa, string cliente);String valor

SELECT venclientes.serie
INTO     :valor
FROM     venclientes  
Where    venclientes.empresa = :empresa
And      venclientes.codigo = :cliente;

Return valor;

end function

