global function string f_cod_pago_venclientes (string empresa, string cliente);String valor

SELECT venclientes.cod_pago
INTO     :valor
FROM     venclientes  
Where    venclientes.empresa = :empresa
And      venclientes.codigo = :cliente;

Return valor;

end function

