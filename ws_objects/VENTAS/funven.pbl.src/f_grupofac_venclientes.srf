global function string f_grupofac_venclientes (string empresa, string cliente);String valor

SELECT venclientes.grupofac
INTO     :valor
FROM     venclientes  
Where    venclientes.empresa = :empresa
And      venclientes.codigo = :cliente;

Return valor;

end function

