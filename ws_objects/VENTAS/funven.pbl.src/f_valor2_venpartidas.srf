global function string f_valor2_venpartidas (string empresa, string codigo);String var

   SELECT venpartidas.valor2
	    INTO :var
    FROM venpartidas  
   WHERE ( venpartidas.empresa = :empresa ) AND  
         ( venpartidas.codigo = :codigo )   ;


RETURN var
//

end function

