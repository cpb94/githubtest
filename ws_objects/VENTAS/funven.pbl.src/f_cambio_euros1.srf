global function decimal f_cambio_euros1 (string ai_codmoneda);dec{6} cambio

  SELECT monedas.euros
    INTO :cambio
    FROM monedas  
   WHERE monedas.moneda = :ai_codmoneda   
  USING sqlca;
  

  
RETURN cambio
end function

