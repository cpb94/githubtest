global function string f_pais_envio (string empresa, string cliente, string envio);String var_pais

SELECT	pais
INTO		:var_pais
FROM		venenvio
WHERE		empresa	=	:empresa
AND		cliente	= 	:cliente
AND		codigo	= 	:envio;

if isnull(var_pais) then var_pais = ""

Return var_pais
end function

