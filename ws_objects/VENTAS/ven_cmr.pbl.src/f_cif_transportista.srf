global function string f_cif_transportista (string empresa, string transportista);String var_cif

SELECT	cif
INTO		:var_cif
FROM		ventransportista
WHERE		empresa	=	:empresa
AND		codigo	= 	:transportista;

if isnull(var_cif) then var_cif = ""

Return var_cif
end function

