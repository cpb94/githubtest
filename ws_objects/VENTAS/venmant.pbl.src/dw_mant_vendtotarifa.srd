 retrieve="  SELECT vendtotarifa.empresa,   
         vendtotarifa.tarifa,   
         vendtotarifa.articulo,   
         vendtotarifa.calidad,   
         vendtotarifa.cantidad,   
         vendtotarifa.dto,   
         articulos.descripcion,   
         calidades.descripcion,   
         ventarifas.descripcion,   
         unidades.abreviado,
		articulos.decimales_unidad  
    FROM vendtotarifa LEFT OUTER JOIN articulos ON vendtotarifa.empresa = articulos.empresa AND vendtotarifa.articulo = articulos.codigo
							LEFT OUTER JOIN calidades ON vendtotarifa.empresa = calidades.empresa AND vendtotarifa.calidad = calidades.codigo 
							LEFT OUTER JOIN ventarifas ON vendtotarifa.empresa = ventarifas.empresa AND vendtotarifa.tarifa = ventarifas.codigo,   
         unidades  
WHERE ( articulos.unidad = unidades.codigo )    
and vendtotarifa.empresa = :empresa
and vendtotarifa.tarifa like :tarifa
and vendtotarifa.articulo like :articulo
and calidades.codigo like :calidad
order by          vendtotarifa.tarifa, articulos.descripcion, vendtotarifa.cantidad
" update="vendtotarifa" updatewhere=0 updatekeyinplace=no arguments=(("empresa", string),("tarifa", string),("articulo", string),("calidad", string)) )
