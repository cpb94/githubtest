         venfac.fcarga,   
         venfac.anyo,   
         venfac.factura,   
         venfac.agrupa,   
         venfac.ffactura,   
         venfac.actualizada,   
         venfac.total_fac,   
         venfac.base_comision,   
         venfac.pais,   
         'S' libre,   
         venfac.total_fac_pts,   
         venfac.generar_recibos,   
         venfac.comision_fija1,   
         venfac.comision_fija2,   
         venfac.comision_fija3  
    FROM venfac  
   WHERE ( ( venfac.empresa = :empresa ) AND  
         ( venfac.serie = :serie  ) AND  
         ( venfac.ffactura between :fdesde   and :fhasta ) )   
ORDER BY venfac.empresa ASC,   
         venfac.anyo ASC,   
         venfac.factura ASC   
" update="venfac" updatewhere=0 updatekeyinplace=no arguments=(("empresa", string),("serie", string),("fdesde", datetime),("fhasta", datetime)) )
