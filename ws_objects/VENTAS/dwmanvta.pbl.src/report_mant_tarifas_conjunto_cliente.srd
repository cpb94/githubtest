 column=(type=char(30) updatewhereclause=yes name=formatos_descripcion dbname="formatos.descripcion" dbalias=".descripcion" )
 retrieve="  SELECT ventarifas_precios.empresa,   
         ventarifas_precios.ventarifas_codigo,   
         ventarifas_precios.ventarifas_activa,   
         ventarifas_precios.articulos_codigo,   
         ventarifas_precios.calidades_codigo,   
         ventarifas_precios.precio,   
         ventarifas.moneda,   
         articulos.descripcion,   
         formatos.descripcion  
    FROM ventarifas_precios,   
         ventarifas,   
         articulos,   
         formatos  
   WHERE ( ventarifas_precios.empresa = ventarifas.empresa ) and  
         ( ventarifas_precios.ventarifas_codigo = ventarifas.codigo ) and  
         ( ventarifas_precios.empresa = articulos.empresa ) and  
         ( ventarifas_precios.empresa = formatos.empresa ) and  
         ( ventarifas_precios.articulos_codigo = articulos.codigo ) and  
         ( articulos.formato = formatos.codigo ) and  
			( not exists ( select * 
								from venclientes_precios 
								where venclientes_precios.empresa = ventarifas_precios.empresa 
								and   venclientes_precios.venclientes_codigo = :arg_cliente
								and   venclientes_precios.articulos_codigo = ventarifas_precios.articulos_codigo
								and   venclientes_precios.calidades_codigo = ventarifas_precios.calidades_codigo ) ) and
         ( ( ventarifas_precios.empresa = :arg_empresa ) AND  
         ( ventarifas_precios.ventarifas_codigo = :arg_tarifa ) AND  
         ( ventarifas_precios.ventarifas_activa = :arg_fecha_activa ) and 
			( ventarifas_precios.articulos_codigo in (:arg_articulos) ) and 
			( isnull(articulos.promocion,'') in (:arg_promocion) ) )    
UNION ALL
  SELECT venclientes_precios.empresa,   
         :arg_tarifa as ventarifas_codigo,   
         :arg_fecha_activa as ventarifas_activa,   
         venclientes_precios.articulos_codigo,   
         venclientes_precios.calidades_codigo,   
         venclientes_precios.precio,   
         ventarifas.moneda,   
         articulos.descripcion,   
         formatos.descripcion  
    FROM venclientes_precios,   
         ventarifas,   
         articulos,   
         formatos  
   WHERE ( venclientes_precios.empresa = ventarifas.empresa ) and  
         ( venclientes_precios.empresa = articulos.empresa ) and  
         ( venclientes_precios.empresa = formatos.empresa ) and  
         ( venclientes_precios.articulos_codigo = articulos.codigo ) and  
         ( articulos.formato = formatos.codigo ) and  
         ( ( venclientes_precios.empresa = :arg_empresa ) AND  
         ( ventarifas.codigo = :arg_tarifa ) AND  
         ( venclientes_precios.venclientes_codigo = :arg_cliente ) and 
			( venclientes_precios.articulos_codigo in (:arg_articulos) ) and 
			( isnull(articulos.promocion,'') in (:arg_promocion) ) )    

