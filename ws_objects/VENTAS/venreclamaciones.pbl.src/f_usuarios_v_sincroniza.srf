global function string f_usuarios_v_sincroniza (string usuario);String variable

  SELECT usuarios.v_sincroniza
    INTO :variable  
    FROM usuarios  
   WHERE usuarios.v_usuario = :usuario   ;
	
	if isnull(variable) then variable = ""

	Return variable

end function

