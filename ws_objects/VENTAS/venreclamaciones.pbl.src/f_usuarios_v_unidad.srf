global function string f_usuarios_v_unidad (string usuario);String variable

  SELECT usuarios.v_unidad
    INTO :variable  
    FROM usuarios  
   WHERE usuarios.v_usuario = :usuario   ;
	
	if isnull(variable) then variable = ""

	Return variable

end function

