 retrieve="  SELECT ventarifas_precios.empresa,   
         ventarifas_precios.precio,   
         calidades.abreviado,   
         formatos.abreviado,   
         ventarifas_precios.calidades_codigo,   
         ventarifas_precios.articulos_codigo,   
         articulos.descripcion,   
         unidades.abreviado  
    FROM ventarifas_precios,   
         articulos,   
         calidades,   
         formatos,   
         unidades,   
         art_codestadistico  
   WHERE ( ventarifas_precios.empresa = articulos.empresa ) and  
         ( ventarifas_precios.empresa = calidades.empresa ) and  
         ( ventarifas_precios.empresa = formatos.empresa ) and  
         ( ventarifas_precios.articulos_codigo = articulos.codigo ) and  
         ( ventarifas_precios.calidades_codigo = calidades.codigo ) and  
         ( articulos.formato = formatos.codigo ) and  
         ( articulos.unidad = unidades.codigo ) and  
         ( articulos.empresa *= art_codestadistico.empresa ) and  
         ( articulos.codigo *= art_codestadistico.codigo ) and  
         ( ( ventarifas_precios.empresa = :empresa ) AND  
         ( ventarifas_precios.ventarifas_codigo = :tarifa ) AND  
         ( ventarifas_precios.ventarifas_activa = :fecha ) AND  
         ( isnull(art_codestadistico.G1,'') in ( :arg_pavrev ) ) AND  
         ( articulos.familia in ( :arg_familias ) ) AND  
         ( articulos.marcas_codigo in ( :arg_marcas ) ) AND  
         ( articulos.submarcas_codigo in ( :arg_submarcas ) ) AND  
         ( articulos.formato in ( :arg_formatos ) ) ) AND  
         isnull(articulos.promocion,'') in ( :arg_promocion )   
ORDER BY ventarifas_precios.empresa ASC   
" retrieve.asneeded=yes arguments=(("empresa", string),("tarifa", string),("fecha", datetime),("arg_pavrev", stringlist),("arg_familias", stringlist),("arg_marcas", stringlist),("arg_submarcas", stringlist),("arg_formatos", stringlist),("arg_promocion", stringlist))  sort="articulos_descripcion A " )
