type m_actualizacambiolineasfras from menu within m_utilidades
end type

event clicked;datastore cabeceras
string sel
long indice,total,anyo,factura,diferentes
dec  cambio,cambio_euros
boolean bien = true

sel = "select anyo,factura,cambio,cambio_euros "+&
		"from venfac where empresa = '"+codigo_empresa+"'"
		
//cabeceras = f_cargar_cursor(sel)	
f_cargar_cursor_nuevo (sel, cabeceras)
total = cabeceras.rowcount()
for indice = 1 to total	
	anyo         = cabeceras.object.anyo[indice]
	factura      = cabeceras.object.factura[indice]
	cambio       = cabeceras.object.cambio[indice]
	cambio_euros = cabeceras.object.cambio_euros[indice]

	select count(*)
	into   :diferentes
	from   venlifac
	where  empresa = :codigo_empresa
	and    anyo    = :anyo
	and    factura = :factura
	and    cambio  <> :cambio;
	
	if isnull(diferentes) then diferentes = 0
	if diferentes > 0 and bien then
		f_mensaje_proceso("Actualizando lineas:",indice,total)
		update venlifac
		set    cambio       = :cambio,
				 cambio_euros = :cambio_euros
		where  empresa = :codigo_empresa
		and    anyo    = :anyo
		and    factura = :factura;
		if sqlca.sqlcode <> 0 then
			bien = false
		end if
	end if
next
if bien then
	commit;
else
	rollback;
	messagebox("Muy","Mal")
end if
destroy cabeceras
end event

