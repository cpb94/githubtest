table(column=(type=long updatewhereclause=yes identity=yes name=año dbname="AÑO" )
 column=(type=long updatewhereclause=yes identity=yes name=mes dbname="MES" )
 column=(type=decimal(0) updatewhereclause=yes identity=yes name=piezas_fabricadas dbname="PIEZAS_FABRICADAS" )
 column=(type=long updatewhereclause=yes identity=yes name=metros2_fabricados dbname="METROS2_FABRICADOS" )
 retrieve="select year(prodpartes_ot.fecha) AÑO, month(prodpartes_ot.fecha) MES, sum (buenas) 'PIEZAS_FABRICADAS', convert ( integer,  sum( ( buenas * formatos.ancho * formatos.largo ) / 10000 ) ) 'METROS2_FABRICADOS'
from prodpartes_ot, articulos, formatos, art_codestadistico
where prodpartes_ot.empresa = articulos.empresa
and prodpartes_ot.articulo = articulos.codigo
and prodpartes_ot.empresa = formatos.empresa
and prodpartes_ot.formato = formatos.codigo
and articulos.empresa = art_codestadistico.empresa
and articulos.codigo = art_codestadistico.codigo
and art_codestadistico.g2 <> 'N'
and articulos.uso = '1'
and prodpartes_ot.operacion = '3'
and prodpartes_ot.empresa = :empresa
and prodpartes_ot.fecha >= :fdesde
and prodpartes_ot.fecha <= :fhasta
and articulos.codigo like :articulo
group by year(prodpartes_ot.fecha), month(prodpartes_ot.fecha)" arguments=(("empresa", string),("fdesde", datetime),("fhasta", datetime),("articulo", string))  sort="año A mes A " )
