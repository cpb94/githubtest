global function string f_tipoiva_comprov (string empresa, string codigo);String 	ls_iva

SELECT comproveedores.tipo_iva
  INTO  :ls_iva  
  FROM  comproveedores
  WHERE (comproveedores.empresa = :empresa ) AND  
        (comproveedores.codigo = :codigo );
		  
Return ls_iva

end function

