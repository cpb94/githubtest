global function decimal f_ppago_comprov (string empresa, string codigo);dec var

  SELECT comproveedores.dtopp INTO :var   
  	FROM comproveedores  
   WHERE comproveedores.empresa = :empresa 
	AND   comproveedores.codigo  = :codigo ;
	
Return var

end function

