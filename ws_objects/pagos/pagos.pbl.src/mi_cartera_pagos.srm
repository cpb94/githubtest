type m_asignacioncuentascambiocartera from menu within m_utilidades
end type

event clicked;long ll_indice,ll_total,ll_anyo,ll_orden,ll_cuentas
string ls_cuenta,ls_sel
datastore ds_datos
	
ls_sel = "select anyo,orden from carpagos where empresa = '"+codigo_empresa+"' order by anyo,orden"

f_cargar_cursor_transaccion(sqlca,ds_datos,ls_sel)
ll_total = ds_datos.rowcount()

for ll_indice = 1 to ll_total	
	f_mensaje_proceso("",ll_indice,ll_total)
	
	ll_anyo  = ds_datos.object.anyo[ll_indice]
	ll_orden = ds_datos.object.orden[ll_indice]
	
	select isnull(count(distinct(isnull(cuenta,''))),0)
	into   :ll_cuentas
	from   carpagos_cuentas
	where  empresa        = :codigo_empresa
	and    anyo  = :ll_anyo
	and    orden = :ll_orden;
	
	if ll_cuentas = 1 then
		select max(isnull(cuenta,''))
		into   :ls_cuenta
		from   carpagos_cuentas
		where  empresa        = :codigo_empresa
		and    anyo  = :ll_anyo
		and    orden = :ll_orden;				
		
		update carpagos
		set    cuenta = :ls_cuenta
		where  empresa = :codigo_empresa
		and    anyo    = :ll_anyo
		and    orden   = :ll_orden;
		
		if sqlca.sqlcode = 0 then
			commit;
		else
			rollback;
			messagebox("Error",string(ll_anyo,"###0")+"-"+string(ll_orden,"#####0"))
		end if
	else
		messagebox("Error",string(ll_anyo,"###0")+"-"+string(ll_orden,"#####0"))
	end if
next

destroy ds_datos
end event

