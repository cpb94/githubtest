event clicked;long anyo,orden,indice,total
string empresa,sel

datastore dw

sel = 'select empresa,anyo_agrupado,orden_agrupado from carpagosdetagrupado group by empresa,anyo_agrupado,orden_agrupado'

dw = f_cargar_cursor(sel)

total = dw.rowcount()

for indice = 1 to total
	f_mensaje_proceso("",indice,total)
	
	empresa = dw.object.empresa[indice]
	anyo    = dw.object.anyo_agrupado[indice]
	orden   = dw.object.orden_agrupado[indice]
	
	update carpagos
	set    agrupado = 'S'
	where  empresa = :empresa
	and    anyo    = :anyo
	and    orden   = :orden;
	
	if sqlca.sqlcode <> 0 then
		rollback;
		messagebox("Error",string(anyo)+"/"+string(orden))
	else
		commit;
	end if
next

destroy dw
end event

