 retrieve="select genter.razon, comproveedores_cuenta.cuenta, comproveedores_cuenta.tipo, sum ( contaapun.debe - contaapun.haber ) saldo, 
( select sum (carpagos.importe) from carpagos where cuenta = comproveedores_cuenta.cuenta and situacion <> '2') cartera
from comproveedores_cuenta, genter, contaapun
where comproveedores_cuenta.empresa = genter.empresa 
and comproveedores_cuenta.codigo = genter.codigo
and genter.tipoter = 'P'
and comproveedores_cuenta.empresa = contaapun.empresa
and comproveedores_cuenta.cuenta = contaapun.cuenta
and contaapun.empresa = :empresa
and contaapun.ejercicio = :ejercicio
group by genter.razon, comproveedores_cuenta.cuenta, comproveedores_cuenta.tipo
order by genter.razon
